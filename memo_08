 from enrol
 where sub_no=101; 
begin
 open t_cursor;
 for
     v_enrol in t_cursor loop
     exit when t_aa%NOTFOUND;
     dbms_output.put_line(v_stu_no||' '||  v_sub_no||' '||v_enr_grade);
 end loop;
 close t_cursor;
end test7;
/

