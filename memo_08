            function Mon(name, id, num){
                this.name=name;
                this.id=id;
                this.num=num;

                this.a=function(){
                    return this.num;
                }
                this.plus=function(money){
                    return this.num+=money;
                }
                this.minus=function(money){
                    return this.num-=money;
                }
            }
        </script>
    </head>
    <body>
        <script>
            var aa=new Mon("tom","132",30000);
