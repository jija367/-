import java.util.Scanner;

class Profile {
	private String name, id;

	Profile(String name, String id) {
		this.name = name;
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public String getId() {
		return id;
	}

}

public class GGG {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("�迭�� ����");
		int n = s.nextInt();
		Profile[] p = new Profile[n];
		for (int i = 0; i < p.length; i++) {
			System.out.println("�̸�,��ȣ");
			p[i] = new Profile(s.next(), s.next());
		}

		while (true) {
			for (int i = 0; i < p.length; i++) {
				System.out.println("�˻��� �̸�");
				String a = s.next();
				if (a.equals("stop")) {
					System.out.println("����");
					System.exit(0);
				} else if (a.equals(p[i].getName())) {
					System.out.println(a + "�� ��ȣ�� " + p[i].getId());

				} else if (!a.equals(p[i].getName()) && i == (n - 1)) {
					System.out.println("�̸� ����");
				}
			}
		}

	}
}




