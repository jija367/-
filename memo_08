set serveroutput on
create or replace procedure test9
(v_sub_no in enrol.sub_no%type)
is
v_stu_no enrol.stu_no%type;
v_enr_grade enrol.enr_grade%type;

cursor tt (v_sub_no number) is -- Ŀ���� ���� ���� ����(v_sub_no�� Ÿ�Ժ���)
    select student.stu_no, enr_grade from student, enrol 
    where student.stu_no=enrol.stu_no and enrol.sub_no=v_sub_no;
begin
    open tt(v_sub_no);
    loop
    fetch tt into v_stu_no, v_enr_grade;
    exit when tt%NOTFOUND;
    dbms_output.put_line(v_stu_no||' '||v_enr_grade);
    end loop;
    close tt;
end test9;
/


