create table emp_sal(empno number(4), ename varchar2(10), workdate date);

create or replace trigger emp_sal_tri
after insert
on emp
    for each row
    begin
    if : new.sal>=5000 then  --���� ������ �ݾ�(new.sal)�� 5õ �̻��̸�
    insert into emp_sal values
   (:new.empno, :new.ename, sysdate);
    end if;
end emp_sal_tri;
