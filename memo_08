----------------------
cursor ����

�޿��� 2000 �̻��� ����� �����ȣ, �̸�, �μ����� �μ��̸� ������ �˻��ϴ� ���ν��� �ۼ�


set serveroutput on
create or replace procedure aa
is
v_empno emp.empno%type;
v_ename emp.ename%type;
v_dname dept.dname%type;

cursor t_aa is
    select empno, ename, dname from emp natural join dept where sal>=2000 order by dname;
begin
    open t_aa;
    loop
        fetch t_aa into v_empno, v_ename, v_dname;
        exit when t_aa%NOTFOUND;
        dbms_output.put_line(v_empno||' '||v_ename||' '||v_dname);
        end loop;
    close t_aa;
end aa;
/


--Ȯ��
execute aa;
        

=> for ������ ����


create or replace procedure test7
is
v_enrol enrol%rowtype;  -- ������ ������ ���� ���ŷο�� ���̺���%rowtype ���� �� ���� 

