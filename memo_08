set serveroutput on
create or replace function f_prof
(f_sub_no in subject.sub_no%type)
return varchar2
is 
f_pr varchar2(10);
begin
select sub_prof into f_pr from subject where sub_no=f_sub_no;
return f_pr;
exception
when no_data_found then 
   return('����');
end f_prof;
/

variable x varchar2(10);

execute :x:=f_prof(999);
execute :x:=f_prof(107);
print x;


