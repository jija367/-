������ȣ�� �����̸��� �Է��Ͽ� �ش� ������ �����̸��� �����ϴ� ���ν����� ������
(������ȣ�� 103, �̸��� ��ö��)


create or replace procedure aa_cust
(v_c_no in cust.c_no%type,
v_c_name in cust.c_name%type)
is
begin
    update cust set c_name=v_c_name where c_no=v_c_no;
end aa_cust;
/

execute aa_cust(103,'��ö��');
select * from cust;



--------------------

������ȣ�� 2�϶� ������ȣ�� �뿩�Ḧ ���ض�(Ŀ�����)


set serveroutput on
create or replace procedure v_rent
is
vv_c_no rent.c_no%type;
vv_r_rent rent.r_rentfee%type;

cursor a_ccc is
select c_no, r_rentfee from rent where v_no=2;

begin
    open a_ccc;
    loop
        fetch a_ccc into vv_c_no, vv_r_rent;
        exit when a_ccc%NOTFOUND;
        dbms_output.put_line(vv_c_no||' '||vv_r_rent);
        end loop;
        close a_ccc;
    end v_rent;
/

execute v_rent;


