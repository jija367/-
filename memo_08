<���� Ǭ��>

import java.util.HashMap;
import java.util.Scanner;

public class GGG {
	public static void main(String[] args) {
		
		Scanner s=new Scanner(System.in);
		HashMap<String,Integer> h=new HashMap<>();
		int max=0;
		String name=null;
		while(true) {
			System.out.println("1. �����Է�"+'\n'+
					"2. �ֿ�� �л� ���"+'\n'+"3.����");
			int a=s.nextInt();
			switch (a) {
			case 1:
				System.out.println("�̸�");
				String n=s.next();
				System.out.println("����");
				int g=s.nextInt();
				h.put(n,g);
				
				
				for(int i=0;i<h.size();i++) {
					if(max<g) {
						max=g;
						name=n;
					}
				}
				break;
			case 2:
				System.out.println("�� ��� �л�: "+name+" "+max);
				break;
			case 3:
				System.exit(0);
			}
		}
	}
}


