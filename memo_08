�а��� �Է¹޾� �� ������ ǥ������ ���
// stddev �Լ�, select ���� �Լ� ȣ��



create or replace function fun(v_dept in student.stu_dept%type)
return number
is 
v_stddev number;
begin 
select stddev(enr_grade) into v_stddev from enrol natural join student where stu_dept=v_dept;
return v_stddev;
end fun;
/


-- �а� �Է��ϴ� �� �ֱ� ������ ��ȣ �ȿ� �־���
select distinct fun('���') from student;


