import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;

public class Db {
	public static Connection con;  // connection �������̽� ����
	public static Connection get() throws SQLException {
		Connection conn=null;  // db �� �����ϱ� ���� �۾�
		try {
			String id="sys as sysdba";  //db ���� ���̵�
			String pw="djaak5685"; // db���� ���
			String url="jdbc:oracle:thin:@172.16.7.59:1521/xepdb1"; 
			// jdbc oracle ��� ���� url
			//jdbc:orcle:thin: �ڿ� @ IPv4 �ּ�(cmd���� ipconfig�� ã�� �� ����)
			//db ��ġ�� �ּ� xepdb1
			
			Class.forName("oracle.jdbc.driver.OracleDriver"); 
			// jdbc ����̹��� �ε��ϴ� �ڵ�
			
			conn=DriverManager.getConnection(url, id, pw);
			//������ �ּ�,���̵�,����� ����̹��� ����
			System.out.println("�����ͺ��̽��� ����ƴ�");
		}catch (ClassNotFoundException e) {
			System.out.println("�ε� ����");
		}catch (SQLException e) {
			System.out.println("������ ���̽� ���� ����");
		}
		
		return conn;
		
	}
	
}




<Oracle Ŭ����>--------------------------

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;



public class Oracle {
	public static void main(String[] args) throws SQLException {
		
		Connection conn=null; //Db�� �����ϴ� �������̽�
		PreparedStatement pstm=null; //sql�� ��ü
		ResultSet r=null; //sql�� ���� ��ȯ
		
		try {
			String que="select * from emp";
			
			conn=Db.get(); // DbŬ������ get() �Լ� ȣ��
			pstm=conn.prepareStatement(que);
			r=pstm.executeQuery();
			
			while(r.next()) {
				String empno=r.getString(1); // 1�� �� ��ȣ(1���� ����)
				String ename=r.getString(2);
				String job=r.getString(3);
				int mgr=r.getInt(4);
				java.sql.Date hiredate=r.getDate(5);
				int sal=r.getInt(6);
				int comm=r.getInt(7);
				int deptno=r.getInt(8);
				
				System.out.println(empno+" "+ename+" "+job+" "+mgr);
				
			}
			
			
		} catch (SQLException e) {
			System.out.println("���ܹ߻�");
			e.printStackTrace();
		}
		r.close();
		pstm.close();
		conn.close();
		
	}
}


