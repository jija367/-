        <script>
            var java=new Object();
            java.ja=9;
            java.html=12;
            java.css=16;
            document.write(java.ja+"<br>");

            var java1={
                ja:9,
                html:12,
                css:16
            };
            document.write(java1.ja+"<br>");

            function Java(){
                this.ja=9;
                this.html=12;
                this.css=16;
            }
            var java2=new Java();
            document.write(java2.ja+"<br>");
        </script>
    </body>
