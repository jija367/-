create or replace procedure test4(v_enr_grade in out enrol.enr_grade%type,
v_stu_no in enrol.stu_no%type,
v_sub_no in enrol.stu_no%type)
is begin
update enrol set enr_grade=enr_grade+v_enr_grade where stu_no=v_stu_no and sub_no=v_sub_no;
select enr_grade into v_enr_grade from enrol where stu_no=v_stu_no and sub_no=v_sub_no;
end test4;
/

variable d_enr_grade number;

begin :d_enr_grade :=10 
end;
/


=> �Ʒ� �ɷ� ������ ����

create or replace procedure test4
(v_sub_no in enrol.sub_no%type,
v_stu_no in student.stu_no%type,
v_enr_grade in out enrol.enr_grade%type)

is 
begin

update enrol
set enr_grade = enr_grade +v_enr_grade
where stu_no=v_stu_no and
      sub_no=v_sub_no;
      
      select enr_grade
      into v_enr_grade
      from enrol
      where stu_no=v_stu_no and
      sub_no=v_sub_no;
      end test4;
      
      /
      
variable d_enr_grade number
begin : d_enr_grade :=10 ;
end;
/

execute  test4(104,20131001,:d_enr_grade);
print d_enr_grade;



