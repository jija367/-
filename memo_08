create table dept_del
(userid varchar2(10),workdate date, deptno number(2), dname varchar2(14), loc varchar2(13));

create or replace trigger dept_tri
after delete -- after �ڿ��� ��𿡼� �̺�Ʈ ó���� �ƴ��� 
    on dept
    for each row  -- ���� ������ 10���̸� ���� ����
    begin -- ���� �̺�Ʈ�� �߻��ϸ� �Ʒ� �̺�Ʈ ó���ϰڴ�
    insert into dept_del(userid, workdate,deptno, dname, loc)
    values(user,sysdate,
            :old.deptno, --old->���� ������(deptno�� ���� �� ������),
            :old.dname, :old.loc);
end dept_tri;
/

-- deptno �� 10���� �� ����-> Ʈ���� �߻�
