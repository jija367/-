class Tv{
	private int size;
	Tv(int size){
		this.size=size;
	}
	protected int getsize() {
		return size;
	}
	
}
class Tv1 extends Tv{
	String a;
	Tv1(int size, String a){
		super(size);
		this.a=a;
	}
	void show(){
		System.out.println(a+" "+getsize()+"��ġ Tv");
	}
}


class Shape{
	int a,b;
	String c;
	Shape(){
		a=1;
		b=3;
	//this(1,3); ���� ������ ȣ���ؼ��� �̿밡��
	}
	Shape(int a,int b){
		this.a=a;
		this.b=b;
	}
	String pr(String c) {
		this.c=c;
		return c;
	}
	void show() {
		for(int i=0;i<a;i++) {
			for(int j=0;j<b;j++) {
				System.out.print(c);
			}
			System.out.println();
		}
	}
	
}
public class TT {
	public static void main(String[] args) {

		Tv1 t=new Tv1(20,"�Ｚ");
		t.show();
	
		System.out.println("����4");
		Shape s=new Shape();
		Shape s1=new Shape(2,5);
		s.pr("@");
		s1.pr("#");
		s.show();
		s1.show();
		
		
	}

}


