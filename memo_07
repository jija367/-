// aa,bb,cc ��� ���

class Per {
	String a;
	Per(String a){
		this.a=a;
	}
}
public class GGG {
	public static void main(String[] args) {
		ArrayList<Per> al=new ArrayList<>();
		al.add(new Per("aa"));
		al.add(new Per("bb"));
		al.add(new Per("cc"));		
		
		Iterator<Per> i=al.iterator();
		while(i.hasNext()) {
			System.out.println(i.next().a);
		}
	}
}


