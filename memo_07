<5,6>


interface Person{
	String work();
}
class Worker implements Person{
	public String work() {
		return "���Ѵ�";
	}
}
class Student implements Person{
	
	public String work() {
		return "�����Ѵ�";
	}
}

abstract class Ab{
	abstract int total(int ary[]);
}
class Cd extends Ab{
	public int total(int ary[]) {
		int sum=0;
		for(int aa:ary) {
			sum+=aa;
		}
		return sum;
	}
}
public class TT {
	static void pr(Person p) {
		System.out.println(p.work());
	}
	public static void main(String[] args) {
	
	System.out.println("����5");
	pr(new Worker());
	pr(new Student());
	
	System.out.println("����6");
	
	Ab c=new Cd();
	System.out.println(c.total(new int[] {1,2,3,4,5}));
	
	}
}

