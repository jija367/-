import java.awt.Color;
import java.awt.FlowLayout;
import java.awt.event.ItemEvent;
import java.awt.event.ItemListener;
import java.awt.event.MouseAdapter;
import java.awt.event.MouseEvent;

import javax.swing.ButtonGroup;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JRadioButton;

class Book{
	String a; int b;
	Book(String a){
		this(a,40000);
	}
	Book(String a, int b){
		this.a=a; this.b=b;
	}
	void title() {
		System.out.println(a+"��"+" "+b+"��");
	}
}

public class GGG extends JFrame {
	GGG(){
		setSize(500,500);
		setLayout(new FlowLayout());
		JRadioButton jr=new JRadioButton("magenta");
		JRadioButton jr1=new JRadioButton("red");
		ButtonGroup b=new ButtonGroup();
		add(jr); add(jr1);
		b.add(jr1); b.add(jr);
		jr.addItemListener(new ItemListener() {
			public void itemStateChanged(ItemEvent e) {
				if(e.getStateChange()==ItemEvent.SELECTED) {
				jr.setBackground(Color.magenta);
				}
				else {
					jr.setBackground(null);
				}
				
			}
		});
		jr1.addItemListener(new ItemListener() {
			public void itemStateChanged(ItemEvent e) {
				if(e.getStateChange()==ItemEvent.SELECTED) {
					jr1.setBackground(Color.red);
					}
				else {
					jr1.setBackground(null);
				}
			}
		});
		
		JLabel j=new JLabel("Hi");
		add(j);
		j.addMouseListener(new MouseAdapter() {
			public void mouseClicked(MouseEvent e) {
				j.setText("�ȳ�");
			}
		});
		
	
		setVisible(true);
	}
	public static void main(String[] args) {
	new GGG();
		
	Book b1=new Book("�ڹ�");
	Book b2=new Book("���̽�",30000);
	b1.title();
	b2.title();
		
	}

}
	// ���ڿ� ��ġ �����ϴ� ���
	// SwingConstants �� ����
	//setBounds(x,y,width,height) 

	//j.setHorizontalAlignment(j.LEFT); 

