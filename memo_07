[toString()]
=> ��ü�� ���ڿ��� ��ȯ �޼ҵ�
=> �Ϲ����� �⺻������(int,double��)�� ���ڿ��� ����Ұ�
=>�Ϲ� �����͸� �����Ϸ��� �������� �⺻ �ڷ� Ŭ����.toString(��)(int �� Integer, double�� Double ��)



class In{
	private int num;
	// private �̾ ��ü�� ���� ������ �Ұ�
	In(int num){
		this.num=num;
	}
	public String toString() { 
		// ��ü���� ���� ���ڿ��� ����� �� �� ����ϴ� �Լ� �⺻ ������!!
		return num+" "; // num�� int �������� ������ �����ָ� ���ڿ��� �ν���
	}
}
public class TT {

	public static void main(String[] args) {
		In i=new In(2);
		// System.out.println(i); 
		// �ٷ� ����ϸ� �޸𸮰��� ��µǾ� �̻��� ���� ����
		System.out.println(i.toString());
		
	}


}


