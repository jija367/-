class Food{
	String menu;
	int price;  // �ʵ�
	
	void pp(int p) {  // ���� �߰�
		price+=p;
		
	}
	void show() {
		System.out.println(price+" "+menu);
	}
}
public class TT {

	public static void main(String[] args) {
		Food f=new Food();
		f.menu="��";
		f.price=1000;
		
		f.pp(1000);
		f.show();
	
		
		Food f2=new Food();
		f2.menu="ġŲ";
		f2.price=2000;
		f2.pp(2000);
		f2.show();
	
	}

}


