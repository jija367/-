<Worker Ŭ����>


public class Worker {
	private int num;
	private String name;
	private String cl;
	private String ad;
	private String bir;
	
	
public int getNum() {
		return num;
	}


	public void setNum(int num) {
		this.num = num;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getCl() {
		return cl;
	}


	public void setCl(String cl) {
		this.cl = cl;
	}


	public String getAd() {
		return ad;
	}


	public void setAd(String ad) {
		this.ad = ad;
	}


	public String getBir() {
		return bir;
	}


	public void setBir(String bir) {
		this.bir = bir;
	}


Worker(int num, String name, String cl, String ad, String bir){
	this.num=num; this.name=name; this.cl=cl; this.ad=ad; this.bir=bir;
}
}



<Show Ŭ����>

import java.util.Scanner;

public class Show {
public static void main(String[] args) {
	Scanner s= new Scanner(System.in);
	Worker w = null;
	while(true) {
		System.out.println("<��� ���� ���� ���α׷�>");
		System.out.println("1.��������Է�");
		System.out.println("2.������� ������ȸ");
		System.out.println("3.���α׷� ����");
		
		int a=s.nextInt();
		
		switch (a) {
		case 1:
			System.out.println("���: ");
			int num=s.nextInt(); 
			System.out.println("�̸�: ");
			String name=s.next();
			System.out.println("�μ�: ");
			String cl=s.next(); 
			System.out.println("�ּ�: ");
			String ad=s.next(); 
			System.out.println("����: ");
			String bir=s.next();
			w=new Worker(num,name,cl,ad,bir);
			System.out.println("��������� �ԷµǾ���");
			break;
		case 2:
			System.out.println("�˻��ϰ��� �ϴ� ����� �����?");
			int b=s.nextInt();
			if(b==w.getNum()) {
				System.out.println("�̸�: "+w.getName());
				System.out.println("�μ�: "+w.getCl());
				System.out.println("�ּ�: "+w.getAd());
				System.out.println("����: "+w.getBir());
			}
			else {
				System.out.println("��������");
			}
			break;
			
		case 3:
			System.out.println("���α׷� ����");
			System.exit(0);
			
		}
		
		
	}
	
	
}
}



