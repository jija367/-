public class TT {
	public static void main(String[] args) {
		Integer i=10;  //�ڵ� �ڽ�
 		Character c='a';
 		Boolean b=true;
 		
 		int a=i;
 		//int a=i.intValue(); ��ڽ�
 		char cc=c;
 		boolean bb=b;
 		
 		//parse�� �����ϴ� �� �� ���ڿ�. ���ڿ��� �⺻Ÿ������ ��ȯ�ϴ� �Լ�
 		int ii=Integer.parseInt("123"); 
 		boolean b2=Boolean.parseBoolean("true"); 
 		double d2=Double.parseDouble("123.12");
 		
 		// valueOf ���ڿ��� �⺻������ ��ȯ.
 		int i3=Integer.valueOf("123"); 
 		
 		int i4= Integer.valueOf("10")+Integer.valueOf("20");
 		System.out.println(i4);  
 		// ���ڿ�10�� 20�� ��üȭ ��Ű�� int i4��� �ڷ������� ��ڽ� ���Ѽ� ����
 		
 		String s=Integer.toString(123); //������ ��ü 123�� ���ڿ��� ��ȯ
 		Integer i2=5;
 		System.out.println(i2.toString());
 		
 		
	}

}

