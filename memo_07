class Animal {
	 String a;

	Animal(String a) {
		this.a = a;
	}

	String ani() {
		return a;
	}
}

class Dog extends Animal{
	String b;
	Dog(String a, String b){
		super(a);
		this.b=b;
	}
	String ani() {


	 return a+" "+b;
	 
	 
	}
}
public class PPP {
	public static void main(String[] args) {
		Animal a1 = new Dog("������","Ǫ��");
		System.out.println(a1.ani());

	}
}


