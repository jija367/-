		l.add("one");
		l.add("two");
		l.add("three");
		Iterator<String> i=l.iterator();
		while(i.hasNext()) {
			if(i.next().compareTo("three")==0) { // ���� �����ϸ� 0 ��ȯ
				i.remove();
				break;
			}
		}
		
		i=l.iterator();
		while(i.hasNext()) {
			System.out.println(i.next());
		}
		}
	}


