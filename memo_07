import java.util.Scanner;

class myChar{
	char ch;
}

public class TT {

	static void swap(myChar a, myChar b) {
// ���� �ȹ�� �κ� myChar a, myChar b
		char imsi;
		 imsi=a.ch;
		a.ch=b.ch;
		 b.ch=imsi;
		 
		System.out.println(a.ch+" "+b.ch);
	}
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		myChar ch1=new myChar();
		myChar ch2=new myChar();
		
		System.out.println("����1 �Է�");
		ch1.ch=s.nextLine().charAt(0);
		System.out.println("����2 �Է�");
		ch2.ch=s.nextLine().charAt(0);
		
		swap(ch1,ch2);
		
		System.out.println("�ٲ� ���� -->"+ ch1.ch+ch2.ch);

	
	}

}

