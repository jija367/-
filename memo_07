class XY{
	private int x,y;
	XY(int x, int y){
		this.x=x;
		this.y=y;
	}
	int getx() {
		return x;
	}
	int gety() {
		return y;
	}
	protected void move(int x, int y) {
		this.x=x;
		this.y=y;
	}
	
}
class XYZ extends XY{
	String a;
	XYZ(int x, int y, String a){
		super(x,y);
				
	}
	void setcolor(String a) {
		this.a=a;
	}
	void setxy(int x, int y) {
		super.move(x, y); // super�� ���� ����
	}
	void show(){
		System.out.println(getx()+","+gety()+"�� "+a+"��");
	}
}
public class TT {
	public static void main(String[] args) {
		
		XYZ xyz=new XYZ(10,10,"red");
		xyz.setxy(20,30);
		xyz.setcolor("blue");
		xyz.show();
		
	}

}


