<���콺 ��Ǹ�����, ���콺����� ���߻��>


import java.awt.Color;
import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
import java.awt.event.MouseAdapter;
import java.awt.event.MouseEvent;
import java.awt.event.MouseListener;
import java.awt.event.MouseMotionAdapter;
import java.awt.event.MouseMotionListener;

import javax.swing.JButton;
import javax.swing.JFrame;

class Mouse extends MouseAdapter implements MouseMotionListener{
	// Listener �� ������ �� �������̽�!!
	public void mouseDragged(MouseEvent e) {
		// ���콺 Ŭ���� ä�� �����϶� ����
		Container c=(Container)e.getSource();
		c.setBackground(Color.yellow);
	}
	public void mouseMoved(MouseEvent e) {} 
	// MouseMotionListener �� �������̽��̱� ������ �߻� �޼ҵ�� ������ �����ؾߵ�
	// �ϳ��� �����Ϸ��� MouseMotionAdapter(Ŭ����) ��ӹ����� ��
	public void mouseReleased(MouseEvent e) {
		// ���콺 �� �� ����
		Container c=(Container)e.getSource();
		c.setBackground(Color.pink);
	}
}
public class TT {

	public static void main(String[] args) {

		JFrame f = new JFrame();
		f.setTitle("java");
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		f.setSize(500,500);
		f.setLayout(new FlowLayout());

		JButton j1 = new JButton("one");
		JButton j2 = new JButton("two");
		Container c = f.getContentPane();
		c.setBackground(Color.pink);
		j1.setBackground(Color.green);
		j2.setBackground(Color.magenta);
		f.add(j1);
		f.add(j2);
		c.addMouseMotionListener(new Mouse());
		c.addMouseListener(new Mouse());
		
		f.setVisible(true);

	}

}



