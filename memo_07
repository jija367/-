// ��ü ����� ���� 1 ����Ŭ���� ����

import java.io.Serializable;


public class PP implements Serializable {

	private String name; // ���� Ŭ���� �������� ���� ����
	private String id;
	private int age;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	
}



