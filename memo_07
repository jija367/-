// Ŭ���� ��� �� ��ĳ���� ����
class Person{}
class Student extends Person{}
class Worker extends Person{}
class ITWorker extends Worker{}
public class TT {
	static void show(Person p) { //Person p=new Student(); �� ����(��ĳ����)
		// �Ű������� ���� �����ؾ��ϹǷ� Ŭ������-> 
		//���ο� �ִ� Ŭ�������� �θ�κ��� ��ӹ����Ƿ� �θ�Ŭ������ ����
		if(p instanceof Person) {  // p�� Person�� ��ü���� Ȯ��
			System.out.println("���");
		}
		if(p instanceof Student) { // p�� Student�� ��ü����Ȯ��
			System.out.println("�л�");
		}
		if(p instanceof Worker) {
			System.out.println("������");
		}
		if(p instanceof ITWorker) {
			System.out.println("IT������");
		}
		
	}
	public static void main(String[] args) {
		show(new Student());
		// instanceof �����ڿ��� Person���� Ȯ���Ҷ� ��ĳ������ �Ǿ� Person�� �´ٰ� ����
		
		show(new Worker());
		show(new ITWorker());
	}

