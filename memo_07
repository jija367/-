import java.util.Scanner;

public class TT {
	static int aa() {
		Scanner s=new Scanner(System.in);
		int a=s.nextInt();
		int b=s.nextInt();
		return a<b?b:a;
	}
	public static void main(String[] args) {
		System.out.println(aa());
		
		// main ���� �Է� ���� �� �Լ��� �ٷ� �Է��ϴ� ���
		// aa(s.nextInt(),s.nextInt()); �̷��� �Է��ص� ��
	}

}

