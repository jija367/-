--������ ������ ����� �����ȣ, ����̸��� ��������� �˻�
select empno, ename from emp natural join dept where loc='NEW YORK' order by empno;

---

--ADAMS����� �ٹ� ���� �μ��̸��� �������˻��϶�.(equi join)
select dname, loc from emp, dept where emp.deptno=dept.deptno and ename='ADAMS';

--�޿��� 2000�̻��� ������� ������� �������˻��϶�. (natural join)
select ename, loc from emp natural join dept where sal>=2000;

--2����equi join���� �ٲ��.
select ename, loc from emp ,dept where emp.deptno=dept.deptno and sal>=2000;

--�޿��� 1000 �̻� 2000 ������ ������� �����ȣ, ����̸�, �μ��̸��� �����ȣ������ �˻��϶�. (join using)
select empno, ename, dname, sal from emp join dept using(deptno) where sal between 1000 and 2000;

--��������� SALESMAN�̸鼭 CHICAGO ������ �ٹ� ���� ������� �˻��϶�.
select ename, loc, job from emp natural join dept where job='SALESMAN' and loc='CHICAGO';

--NEW YORK�̳� DALLAS ������ �ٹ��ϴ� ������� �����ȣ�� ����̸��� �����ȣ ������ �˻��϶�. (equi join)
select empno, ename from emp, dept where emp.deptno=dept.deptno 
and loc in ('NEW YORK', 'DALLAS') order by empno;

--�μ��̸��� ACCOUNTING�̰ų�, ������ CHICAGO�� ����� �����ȣ�� ����̸��� �˻��϶�. (equi join)
select empno, ename, dname, loc from emp, dept where emp.deptno=dept.deptno and (dname='ACCOUNTING' or loc='CHICAGO');

--NEW YORK�̳� DALLAS ������ �ٹ��ϴ� ������� �����ȣ�� ����̸��� �����ȣ ������ �˻��϶�. (natural join)
select empno, ename, loc from emp natural join dept where loc in('NEW YORK','DALLAS') order by empno;

--BLAKE ������� ���� �޿��� �޴� ����̸����˻��϶�. (������)
select ename, sal from emp where sal>(select sal from emp where ename='BLAKE');

--FORD ����� ���� �μ��� �ٹ��ϴ� ����̸��� �˻��϶�. (������)
select ename, deptno from emp where deptno=(select deptno from emp where ename='FORD') and ename<>'FORD';


--11. FORD ����� ���� �޿��� �޴� ����� �����ȣ���˻��϶�. (������)
select empno from emp where sal=(select sal from emp where ename='FORD') and ename<>'FORD';

--12. �μ��̸��� ��SALES���̸鼭 ��������� ��MANAGER���� ����� �����ȣ, ����̸��� ����̸� ������ �˻��϶�. (natural join)
select empno, ename from emp natural join dept where dname='SALES' and job='MANAGER' order by ename;

--13. SCOTT ������� ���� �޿��� �޴� ��� �������˻��϶�. (������)
select * from emp where sal>(select sal from emp where ename='SCOTT');

--14. ALLEN ������� ���� �޿��� �޴� ��� �������˻��϶�. (������)
select * from emp where sal<(select sal from emp where ename='ALLEN');

--15. ��ü ����� ��� �޿����� �޿��� ���� ��� ������ �˻��϶�. (������)
select * from emp where sal>(select avg(sal) from emp);


--16. HICAGO ������ ��ġ�ϴ� �μ��� �ٹ��ϴ»�� ������ �˻��϶�. (������)
select * from emp where deptno=(select deptno from dept where loc='CHICAGO');



