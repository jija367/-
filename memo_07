	public static void main(String[] args) {
		ArrayList< Profile> ar=new ArrayList<>();
		ar.add(new Profile("dd",1));
		ar.add(new Profile("ff",2));
		ar.add(new Profile("ss",3));
		for(int i=0;i<ar.size();i++) {
			Profile p=ar.get(i);
			System.out.println(p.age+" "+p.id);
		}
		
	}
