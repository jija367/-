[StringTokenizer Ŭ���� ����]

���ڿ��� ������ �۾�


import java.util.StringTokenizer;

public class TT {
	public static void main(String[] args) {
		String str = "id=123#name=gildong#add=seoul";

		StringTokenizer s = new StringTokenizer(str, "#");
		// ���ڿ� ������ �۾�
		int n = s.countTokens(); // ���� ���ڿ��� ����� int ������ �����ϴ� �۾�
		System.out.println(n);

		while (s.hasMoreTokens()) { // s�� �����ִ� ����� ���� ������
			String a = s.nextToken(); // s�� ����� ���ڿ� a�� ����
			System.out.println(a);
		}

	}

}


