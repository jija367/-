import javax.swing.JFrame;

public class PP extends JFrame {
	PP(){
//setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
// ������ �־ �ǰ� �ȳ־ ��. �ݱ⸦ ������ ����ǰ� ��
// �ȳ����� �ܼ��� �ݾƵ� �ܼ��� ��� �������� ��
		setTitle("Java");  
		// ���� ����  super("Java"); ��� �ص� ��
		setSize(500,300); // ������, ���γ��� ����
		setVisible(true); // ������ ��� �޼ҵ�-> ������ ��¾ȵ�
	}
	public static void main(String[] args) {
		new PP();
	}
}


