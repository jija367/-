//�����޽��� ���� �����غ���


import java.util.Scanner;

public class TTTT {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b = s.nextInt();
		int c;
		try {
			if (b == 0) {
			throw new Exception("0���� ����������? �ȵ˴ϴ�");
			// ���� ���� ���� �޽���
	        // throw new Exception->if������ ���̸� ������ϰ� 
			//Exception���� �ѱ�ڴ�(catch�� �����ٴ� �ǹ�)
			}
			c= a / b; 
			// try ������� if�� ���� ������ 
			//�����޽��� ��� ���� ���� �õ��Ͽ� �ۼ��� �޽����� �ȳ���!
			System.out.println(c);

		} catch (Exception e) {
			System.out.print("�߻�����: ");
			System.out.println(e.getMessage()); // ������ ���� �����޽���
		} finally {
			System.out.println("���� ������");
		}
	}

}



