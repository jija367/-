class Circle{
	int r;
	private String name;  // private ������ �ϸ� �ش� Ŭ���� �ȿ����� ��밡��
	double area(){
		return 3.14*r*r;
	}
	void show() {
		System.out.println(name);
	}
}

public class TT {

	public static void main(String[] args) {
	
		Circle c=new Circle();
		c.r=10;
		c.name="����";  // �����̺����� �༭ ���� �Ұ�
		double a=c.area();
		System.out.println(a);
		c.show(); 
	}

}



