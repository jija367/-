abstract class Ab {

	char pr(char a, int b) {

		for (int i = 1; i < b; i++) {
			System.out.print(a);
		}
		return a;

	}

}

class Aa extends Ab {


}

public class TT {
	public static void main(String[] args) {

		Ab a = new Aa();
		// a.pr('$',10);
		System.out.println(a.pr('$', 10));
	}
}

