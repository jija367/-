public class GGG {
	static <T> void show(T[] a) { // ���׸� �޼ҵ�
		for(int i=0;i<a.length;i++) {
			System.out.println(a[i]);
		}
	}
	public static void main(String[] args) {
		
		String str[]=new String[] {"java","db","c++"};
		show(str);
		
		
	}
}


