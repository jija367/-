// ��� �� instanceof ��ü�� ����

class Fruit {
	void fruit() {
		System.out.println("����");
	}

}

class Apple extends Fruit {
	void apple() {
		System.out.println("���");
	}

}

class PineApple extends Apple {
	void pineapple() {
		System.out.println("���ξ���");
	}
}

public class TT {
	static void pr(Fruit fr) {
		if (fr instanceof PineApple) {
			// fr�� PineApple�� ��ü���� Ȯ��
			((PineApple) fr).pineapple();
			// fr.pineapple �� �Է��ص� �ڵ� �ٿ�ĳ����
// fr�� �θ�Ŭ������ ��ü���� pineapple(); �޼ҵ忡 ���ٺҰ�. ������ �ٿ�ĳ����
		} else if (fr instanceof Apple) {
			((Apple) fr).apple();
		} else {
			fr.fruit();
		}
	}

	public static void main(String[] args) {
		Fruit f = new Fruit();
		Apple a = new Apple();
		PineApple p = new PineApple();

		pr(f);
		pr(a);
		pr(p);

	}
}


