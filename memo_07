// ��ü �迭[3] ����
// ������ �Է� �޾Ƽ� �迭�� ������ �� ���� ���

import java.util.Scanner;

class Circle{
	private double r;
	Circle(double r){
		this.r=r;
	}
	double area() {
		return 3.14*r*r;
	}
}
public class GGG {
	public static void main(String[] args) {
		Circle c[]=new Circle[3];
		Scanner s=new Scanner(System.in);
		
		for(int i=0;i<c.length;i++) {
			c[i]=new Circle(s.nextDouble());
			System.out.println(c[i].area());
		}
		
	}
}




