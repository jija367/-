t2.start();  // ��ŸƮ ����


<������ �ΰ�>

class Th extends Thread {
	String str;

	Th(String str) {
		this.str = str;
	}

	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.println(str);

			try {
				Thread.sleep(100);
			} catch (Exception e) {
			}
		}
	}
}


public class PPP {
public static void main(String[] args) {
	Th t=new Th("dd");
	t.start();
}
}



---


class Th implements Runnable{
 String str;
 Th(String str){
  this.str=str;
 }
 public void run() {  //�������� ���θ޼ҵ�
  for(int i=0;i<10;i++) {
   System.out.println(str);
  try {
   Thread.sleep(100);
  }
  catch(Exception e) {}
  }
 }
}
public class FriendNum {
 public static void main(String[] args) {
  
  Th t=new Th("������");
  Thread t1=new Thread(t);
  t1.start();
 }

}


---

Runnable ����


class Total{
	int sum;
	Total(){
		sum=0;
	}
	void add(int n) {
		sum+=n;
	}
	int getsum() {
		return sum;
	}
}
class Add extends Total implements Runnable{
	int a,b;
	Add(int a, int b){
		this.a=a;
		this.b=b;
	}
	public void run(){
		
		for(int i=a;i<=b;i++) {
			add(i);
		}
	System.out.println(getsum());
	}
}
public class PPP {
public static void main(String[] args) {
	Add a1=new Add(1,10);
	Add a2=new Add(1,100);
	Thread t1=new Thread(a1);
	Thread t2=new Thread(a2);
	t1.start();
	t2.start();
	
	try {
		t1.join();  
		// ���߿� ������ �۾��� ���� ��µ��� �ʰ� ����ִ� �۾�
		// �������� �ʰ� try�� ������.
		t2.join();
	}catch(Exception e) {}
	
}
}

