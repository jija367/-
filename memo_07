import java.util.Vector;

public class GGG {
	public static void main(String[] args) {
		Vector<Integer> v=new Vector<Integer>();
		v.add(new Integer(5));
		//�⺻ ������ 5�� ��üȭ ���� ���Ϳ� ����
		v.add(5); //-> �ڵ��ڽ����� �̷��� ��� ��
		v.add(10);
		v.add(0,20); // �ε��� 0�� 20�� �ְڴ�(��ġ����)
		System.out.println(v.size());
		System.out.println();
		for(int i=0;i<v.size();i++) { //���ʹ� length�Լ� ����
			int n=v.get(i); //���Ϳ��� ������
			System.out.print(n+" ");
		}
	}
}


