public class TT0629TT {

	

	public static void main(String[] args) {

		
		try {  // ������ �� �ڵ带 �Է��ؼ� ������ ������ ���, ������ catch�� �Ѿ
			// ���� Ȯ�ν� ���������� �˻���. 
			//�Ʒ��� ������ �־ ������ �ɷ����� �ٷ� �ش� �������� catch�� ���޵�
		int aa[]=new int[3];
		aa[3]=100;
		int a=10/0;
		System.out.println(aa[3]);
		System.out.println(a);
		}
		catch(Exception e){ 
			e.printStackTrace();
			// e.printStackTrace(); : e�κ��� ��𿡼� ������ ������ ����ض�
			//java.lang.ArithmeticException: / by zero <-0���� ���� �� ����
			//�Ϲ� syso ����ϸ� ���� ���� �ȶ߰� �Է��� ���� ��µ�
		}
		finally { // ������ ���縻�� ������ ��µ�
			System.out.println("���� ������");
		}
	}

}


