<����Ǭ��>
interface Cal {
	int total(int a, int b);

	int big(int a, int b);
}

class Calcu implements Cal {
	int sum=0;
	public int total(int a, int b) {

		for (int i = a; i <= b; i++) {
			sum+= i;
		}
		return sum;
	}

	public int big(int a, int b) {
		return (a > b) ? a : b;
	}
}

class Th implements Runnable {
	String a;

	Th(String a) {
		this.a = a;
	}

	public void run() {
		System.out.println(a);
	}
}

class Timer extends Thread {
	public void run() {
		for (int i = 0; i < 10; i++) {
			System.out.print(i + 1 + " ");
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
			}
		}
	}
}

class Total {
	int sum;

	Total() {
		sum = 0;
	}

	void total(int n) {
		sum += n;
	}

	int get() {
		return sum;
	}
}

class AThread extends Thread {
	Total t;
	int a, b;

	AThread(Total t, int a, int b) {
		this.t = t;
		this.a=a;
		this.b = b;
	}

	public void run() {
		for (int i = a; i <= b; i++) {
			t.total(i);
			//sum += i
		}
		System.out.println(t.get());
	}
}
interface Figure{
	
	 void circle_area();
	 void rec_area();
}
class Circle implements Figure{
	int a;
	int b;
	Circle(int a){
		this.a=a;
	}
	public void circle_area() {
		System.out.println(a*a*3.14);
	}
	public void rec_area() {
	System.out.println(a*b);
	}
}
class Rec implements Figure{
	int a,b;
	Rec(int a, int b){
		this.a=a; this.b=b;
	}
	public void circle_area() {
		System.out.println(a*a*3.14);
	}
	public void rec_area() {
		System.out.println(a*b);
	}
}
public class TT {
	public static void main(String[] args) {
		System.out.println("����1");
		Calcu c = new Calcu();
		System.out.println("����: " + c.total(1, 3) + " ū��: " + c.big(1, 10));

		System.out.println("����2");
		Th t = new Th("������ 1");
		Thread th = new Thread(t);
		th.start();

		System.out.println("����3");
		Timer tt = new Timer();
		try {
		tt.start();
		tt.join();
		}catch(Exception e) {}
		
		System.out.println();
		System.out.println("����4");
		Total ttt = new Total();
		AThread a = new AThread(ttt, 1, 50);
		AThread b = new AThread(ttt, 51, 100);
		try {
			a.start();
			a.join();
			b.start();
			b.join();
		} catch (Exception e) {
		}
		
		System.out.println("����5");
		Figure f1=new Circle(5);
		Figure f2=new Rec(2,5);
		f1.circle_area();
		f2.rec_area();
	}

}


