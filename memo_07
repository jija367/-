class Car{
	static int count=0;  // Ŭ���� ����, �ʱⰪ=0
	
	Car(){  // �⺻������
		count++;
	}
}


public class TT {
	public static void main(String[] args) {

	Car mycar1=new Car();  // �⺻ �����ڿ��� count++ �Ͽ� �̹� 1�� ����
	System.out.println("���� ����� �ڵ��� ����: "+mycar1.count); // ������ 1���
	Car mycar2=new Car(); // ������ ������ 1�� �⺻������ ++ �� ������ 2�� ����
	System.out.println("���� ����� �ڵ��� ����: "+mycar2.count); // 2���
	Car mycar3=new Car();
	System.out.println("���� ����� �ڵ��� ����: "+mycar3.count);
	System.out.println(Car.count);  
	// static�� ���� Ŭ���� ������ ������ �Ǿ� 
	//��ü.������ �������� �ʰ� Ŭ���������ε� ���� ����
	
	}

}


<Ŭ�������� ����>

class AA {
	static int n = 0; // Ŭ��������

	AA() {
		in(); // �޼ҵ� ȣ��
	}

	void in() { // �޼ҵ�
		n++;
	}
}

public class TT {
	public static void main(String[] args) {

		AA a = new AA();
		// ������ ȣ��->������ �ȿ� �޼ҵ� ȣ���� ����=> n++�̱� ������ n=1
		a.n++; // ���� �����ڷκ��� �̹� 1�� ������ ���¿��� ++�ž� 2
		AA.n++; // ������ ++�ż� 3
		System.out.println(AA.n); // 3���

	}

}


[0707]


// Ŭ���� ���� ����
class Car{
	public static int max(int a, int b) {
		return (a>b)?a:b;
	}
	public static int min(int a, int b) {
		return (a<b)?a:b;
	}
}
public class TT {
	public static void main(String[] args) {
		
		Car c=new Car();
		System.out.println(c.max(3, 8));  // ��ü�� ����
		System.out.println(Car.min(3, 8)); 
		// Ŭ������ �ٷ� ����(static�� ���� Ŭ���� �����̱� ������ ����)

	}

}

