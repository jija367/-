// Ŭ���� ��ӿ���

class XY {
	private int x, y;

	void set(int x, int y) { // ����. �Ű����� �ʱ�ȭ �۾�
		this.x = x;
		this.y = y;
	}

	void show() {
		System.out.println(x + " " + y);
	}
}

public class T0707 {
	public static void main(String[] args) {
		XY x=new XY();  // �����ڰ� ���� () �ȿ� �� ���� �� ����
		// �Ű������� private �̱� ������ ���� ���� �Ұ�
		x.set(1,2); // �Լ��� ���� ���� �����Ͽ� �ʵ忡 �� �Է�
		x.show(); // �Է� �� �� ���
		
				
	}
}


