[�������̽� ���߻��]

�������̽� �������� ���߻�� ������
A,B�� �������̽�, C�� ��ӹ޴� Ŭ������� �ϸ�
->  C implements A,B {}
�� ������ ������

�׷��� C�� ������ �������� �ʰ� �߻�޼ҵ常 ����� ���� ��� 
implements �� �ƴ� extends�� ��� ����

-> interface C extends A,B {}


--------

// �������̽� ���߻��, ��ĳ����

interface A{ 
	public void funcA();  // �߻�޼ҵ�
} 
 interface B{
	public void funcB(); //�߻�޼ҵ�
}
 interface C extends A,B{  // ���߻��
	 // A,B�� ������ �����Ұ� �ƴ϶��(C�� interface�̱� ����) extends ���, 
	 //�����ҰŸ� implements ���
	public void funcC(); // �߻�޼ҵ�
}
class D implements C{ // ��ӹ޾Ƽ� ������ Ŭ����
	// C�� �̹� A,B�� ��� �޾Ƽ� C�� ���
	public void funcA() {
		System.out.println("funcA");
	}
	public void funcB() {
		System.out.println("funcB");
	}
	public void funcC() {
		System.out.println("funcC");
	}
}
public class TT {
	
	public static void main(String[] args) {
		
		D d1=new D();
		// �������̽��� ��ü ������ �Ұ���-> ��� ���� D���� ��ü �����ؾߵ�
		A a1=d1; //�������̽� ��ü ���� ������ �Ұ����ϳ� �� ĳ�������� ���� ����
		a1.funcA();
		
		B b1=d1;
		b1.funcB();
		
		C c1=d1;  // C�� �����޾ұ� ������ funcA,B,C ��� ������ ������
		c1.funcC();
		c1.funcA();
		c1.funcB();
	}

}

