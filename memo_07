--1. �л����̺��� �������̺��� natural join�϶�.
select * from student natural join subject;

--2. �����̸��� �й�, ������ �˻��϶�.(natural join)
select sub_name, enrol.stu_no, enr_grade from subject natural join enrol;

--3. �����̸��� �й�, ������ �˻��϶�.(join~using)
select sub_name, stu_no, enr_grade from subject join enrol using(sub_no);

--4. ������ 70�� �̻��� �л� �̸��� �˻��϶�.(equi join)
select stu_name from student, enrol where student.stu_no=enrol.stu_no and enr_grade>=70;

--5. ������ 60�� �̻��� �л� �̸��� �˻��϶�.(join~using)
select stu_name from student join enrol using(stu_no) where enr_grade>=60;

--6. ������ 70�� ������ �л� �̸��� �˻��϶�.(natural join)
select stu_name from student natural join enrol where enr_grade<=70;


--7. ������ ������ �����ϴ� ������ �����ϴ� �л��� �̸��� �˻��϶�.(equi join)
select stu_name from student, enrol, subject where student.stu_no=enrol.stu_no and
enrol.sub_no=subject.sub_no and sub_prof='������';


--8. ��ǻ�Ͱ����� �����ϴ� �л����� �й��� �̸��� �˻��϶�.(equi join)
select student.stu_no, stu_name from student, subject, enrol where student.stu_no=enrol.stu_no and
enrol.sub_no=subject.sub_no and sub_name = '��ǻ�Ͱ���';


