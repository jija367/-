import java.util.Scanner;

public class TT0629TT {

	static void big(double a, double b) {
		if(a<b) {
			System.out.println(b);
		}
		else
		{
			System.out.println(a);
		}
	}
	public static void main(String[] args) {
		
		big(5.5,6.5);
		
		
	}

}



