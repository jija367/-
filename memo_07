import java.util.Iterator;
import java.util.Vector;

public class GGG {
	public static void main(String[] args) {
		Vector<Integer> v=new Vector<>();
		v.add(1);
		v.add(2);
		v.add(3);
		
		Iterator<Integer> it=v.iterator();
		while(it.hasNext()) { // it �� ���� ���� ������
			int n=it.next(); // ���� �����͸� n�� ����
			System.out.println(n); // ���
		}
	}
	
}


