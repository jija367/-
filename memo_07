	for(int i=0;i<c.length;i++) {
		c[i]=new Circle(s.nextDouble());
		System.out.println(c[i].area());
	}
	
	System.out.println("����3");
	Num nb=new Num();
	nb.add(7); nb.add(-1); nb.add(20); nb.add(5);
	
	System.out.println("Sum = "+nb.getSum());
	System.out.println("Max = "+nb.getMax());
	
	Num nb1=new Num(3);
	System.out.println("nb1 Sum = "+nb1.getSum());
	System.out.println("nb1 Max = "+nb1.getMax());
	
	
	
	System.out.println("����4");
