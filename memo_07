import java.util.Iterator;
import java.util.TreeSet;

class Pro implements Comparable<Pro>{
	String name;
	int id;
	Pro(String n, int i){
		name=n; id=i;
	}
	void show() {
		System.out.println(name+" "+id);
	}
	public int compareTo(Pro p) { 
		if(id>p.id)
			return 1;
		else if(id<p.id)
			return -1;
		else
			return 0;
	}
}

public class GGG {
	public static void main(String[] args) {
		
		TreeSet<Pro> t=new TreeSet<>();
		t.add(new Pro("dd",10));
		t.add(new Pro("ff",5));
		t.add(new Pro("ss",4));
		// Ʈ������ ������������ ��µ�
		// public int compareTo(Pro p) �ε�ȣ �ݴ�� �ϸ� ���� ���
		Iterator<Pro> i=t.iterator();
		while(i.hasNext()) {
			i.next().show();
		}
	}
}


