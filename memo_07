public class TT0629TT {
	
	public static int plus(int v1, int v2) {
		// �Լ�����. return�� ������ ����� ��������
		int result;
		result=v1+v2;
		return result;
	}
	
	public static void main(String[] args) {
		
		int hap=plus(100,200);
// �Լ� �ڷ����� �����ؾߵ�, �Ű����� �� �����ؾߵ�!
		//�Լ� ȣ���� ������ �Լ� ����Ұ�
		System.out.println(hap);
	}

}


