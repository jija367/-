import javax.swing.SwingConstants;


public class TT extends JFrame {
	JCheckBox ch=new JCheckBox("Ȱ��ȭ");
	JButton b=new JButton("��ư");
	JButton b1=new JButton("�α���");	
	JRadioButton jr=new JRadioButton("�ڹ�");
