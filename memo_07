[��������]

�ٷ� ���ڸ��� ���Ͽ� ���� �� ũ�� �ڸ� �ٲ�

3 2 1 6 5
2 3 1 6 5
2 1 3 6 5   -> 1ȸ��
2 1 3 5 6
--------------------
1 2 3 5 6   -> 2ȸ��  ����




// ���� ����

public class TT0629TT {

	public static void main(String[] args) {
		int a[] = { 7, 5, 2, 1, 9 };
		int temp; // �ڷᰪ ������ �� ���� ����
		for (int i = 0; i < 4; i++) { // �տ��� i, �ڿ� ���Ͽ� �ٲٴ� �۾����� �� 4�� ����
			for (int j = i + 1; j < 5; j++) { // ���ڸ��� j
				if (a[i] > a[j]) { // a[0]>a[1] ���ϴ� �۾�
					temp = a[i]; // a[0]�� �� ũ�ٸ� temp��� �� ������ a[0]�� ���� ����
					// �� �迭�� ���� 5�� 7�� ���Ͽ� 7�� �� ũ�Ƿ� temp=7�� ��
					a[i] = a[j]; // a[0]������ a[1]�� ���� ����
				// �� �迭 ������ a[1]=5���̹Ƿ� a[0]=5�� ��, a[1]�� ���� a[0]�� �־��ذ�
					a[j] = temp; // a[1]������ temp�� �־��� a[0]�� ���� ����
				// a[1]���ٰ� �ٽ� temp�� �ִ� 7 ���� �־���. �� a[1]=7 �� ��
				// a[i]�� �ִ� �ڷᰪ�� �� ������ temp�� �ְ� �ٽ� a[j]�� �ű�� �۾�
				// �̷������� �ݺ��۾��Ͽ� �迭 ����
				}
			}
		}
		// ������ �迭 �ȿ� �ִ� ���� ��� ��ȯ�ϰ� �Ʒ� ���� for���� ���� ���
		for (int i = 0; i < a.length; i++) {
			System.out.print(a[i]);
		}
	}

}



���ڸ��� 4�� ���� �� ���ذ��µ� ���ڸ��� 5���ΰ� �� �򰥸�
