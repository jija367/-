import javax.swing.JTextField;

class Text implements ActionListener{
	JTextArea jt;
	Text(JTextArea jt){
		this.jt=jt;
	}
	public void actionPerformed(ActionEvent e) {
		jt.setText(" "); // �ʱ�ȭ
		jt.append("���� �Է�");
	}
}
public class GGG {
	public static void main(String[] args) {
	JFrame j=new JFrame();
	j.setLayout(new FlowLayout());
	j.setSize(300,300);
	j.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
	j.setVisible(true);
	
	JTextArea ja=new JTextArea(10,20);
	ja.append("���� �Է�");
	ja.setLineWrap(true);  // �ڵ� �ٹٲ� ����
	
	JButton b=new JButton("������ư");
	b.addActionListener(new Text(ja));
	
	j.add(ja);
	j.add(b);
	
	
	}
}


