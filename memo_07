		FileWriter out1=new FileWriter("test0.txt");
		BufferedWriter outt1=new BufferedWriter(out1);
		while(true) {
			System.out.println("�̸�");
			str=in.readLine();
			if(str.equals("�׸�")) {
				break;
			}
			outt1.write(str);
			System.out.println("id");
			str1=in.readLine();
			outt1.write(str1);
			if(str1.equals("�׸�")) {
				break;
			}
			
		}
		outt1.close();
		in.close();
		
		
		System.out.println("����3.1");
