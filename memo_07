class Music{
	static String title, singer;
	Music(String title, String singer){
		this.title=title;
		this.singer=singer;
	}
	Music(String title){
		this(title,singer);
		this.title=title;
	}
}

class Cal{
	int a,b;
	
	static int add(int a, int b) {
		return a+b;
	}
	static int div(int a, int b) {
		return a/b;
	}
}

public class PP {
public static void main(String[] args) {
	
	Music m=new Music("On", "BTS");
	System.out.println(m.title+" "+m.singer);
	Music m1=new Music("00:00");
	System.out.println(m1.title+" "+m1.singer);
	
	System.out.println("����4");
	
	Cal c=new Cal();
	Cal c1=new Cal();
	
	int a=Cal.add(10,5);
	int b=Cal.div(10,5);
	System.out.println(a+" "+b);
	
	
	System.out.println("����5");
	
	for(int i=0;i<4;i++) {
		for(int j=0;j<3-i;j++) {
			System.out.print(" ");
		}
		for(int j=0;j<i*2+1;j++) {
			
			System.out.print(j);
		}
		System.out.println();
	}
	
}
	
}


