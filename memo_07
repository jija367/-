//TreeSet �޼ҵ� ��� ����


import java.util.Iterator;
import java.util.TreeSet;

public class GGG {
	public static void main(String[] args) {
	TreeSet<Integer> grade=new TreeSet<Integer>();
	grade.add(10);
	grade.add(100);
	grade.add(5);
	
	Integer i;
	i=grade.first(); // ������
	System.out.println(i);
	
	i=grade.last(); // �ְ���
	System.out.println(i);
	
	i=grade.lower(new Integer(80)); // 80������ ���� ����-> ���� ����� ��
	System.out.println(i);
	
	i=grade.higher(new Integer(80)); // 80������ ���� ����-> ���� ����� ��
	System.out.println(i);
	}
}


