	String a;
	Song(String a){
		this.a=a;
	}
	String getsong() {
		return a;
	}
}

public class P0707 {

	public static void main(String[] args) throws IOException {
		System.out.println("����1");
		Scanner s=new Scanner(System.in);
		int c=0;
		double sum=0;
		while(true) {
		double a=s.nextDouble();
			if(a==-1) {
				break;
			}
			sum+=a;
			c++;
		}
		System.out.println(sum/c);
		
		
		System.out.println("����2");
		
