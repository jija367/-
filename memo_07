//total(1,10); / total(1,100); / 1~10, 1~100 �������
public class TT0629TT {
	static void a(int a, int b) {
		int sum = 0;
		for (int i = a; i <= b; i++) {
			sum += i;
		}
		System.out.println(sum);
	}

	public static void main(String[] args) {

		a(1, 10);
		a(1,100);
	}

}



