//�������̽� ���߻�� ����

class Tv {
	public void on() {
		System.out.println("Ƽ�� ��");
	}
}
interface Computer{
	public void m();  // �߻�޼ҵ�, ���� ����
}
class Com{
	public void m() { 
		// implements �� ���� ������ �̸��� ���� Computer �� �߻�޼ҵ�� �ٸ� �Լ�
		System.out.println("��");
	}
}
class Ipad extends Tv implements Computer{
	// Tv�� Ŭ�������� extends ���, Computer �� �������̽����� implements ���
	Com c=new Com();
	public void m() { // �������̽��� �߻�޼ҵ� ����
		c.m(); 
	// Com�� �ִ� m �Լ� ȣ��->System.out.println("��"); ȣ���Ͽ� ����϶�� �ǹ�
	}
	public void ip() {
		m();
	// �ٷ� ������ ������ m�Լ� ���(Com�� m�� ȣ���Ͽ� ����ϴ� �Լ�)
		on();
	}
}
public class TT {

	public static void main(String[] args) {
	
		Ipad i=new Ipad();
		Tv t=i;
		Computer c=i;
		// �������̽� ��ü �������� �Ұ�, ipad ��ü�� ��ĳ�����Ͽ� ����
		i.ip();
				
	}

}

