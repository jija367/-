import java.util.Scanner;

public class TT {

	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		int a[] = new int[10];
		int b;
		for (int i = 0; i < 10; i++) {
// ���� �迭�� �Է°� 10�� �־���
			a[i] = s.nextInt();
			
		}
			for ( int i = 0; i < 9; i++) {
// �տ����ڴ� �ڿ����ڿ� ���ϹǷ� �� ����-1���� �ݺ�
				for (int j = i+1; j < 10; j++) {
// �ڿ����ڴ� �� ���ڿ� ���ϹǷ� �ռ���+1 �� �����ؼ� ������ ���ϹǷ� �Ѱ�������.

					if (a[i] > a[j]) {
						b = a[i];
						a[i] = a[j];
						a[j] = b;
					}
				}
			}
		
		for (int i = 0; i < 10; i++) {
			System.out.print(a[i] + " ");
		}
	}

}


