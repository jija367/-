import java.util.Scanner;

class Circle {
	private double r;

	Circle(double r) {
		this.r = r;
	}

	double area() {
		return 3.14 * r * r;
	}
}

public class TT {

	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		Circle c[] = new Circle[5];
		double sum=0;
		for (int i = 0; i < 5; i++) {
			double a = s.nextDouble();
			c[i] = new Circle(a);

		}
		for(int i=0;i<5;i++) {
			sum+=c[i].area();
		}
		System.out.println(sum);

	}

}


