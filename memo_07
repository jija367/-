public class TT0629TT {

	static int pp(int a, int b) { // ��ȯ���� int �̹Ƿ� �Լ� ȣ��� ������ int������ ���� ������
		System.out.println("dkdk"); // ���� �� ����� syso�� ����� ���뵵 ���� �����
		return 100; // ��½� �Ű������� ��� �����Ƿ� ������ 100���� ����
	}

	public static void main(String[] args) {
		int a = pp(1,2); 
		// �Ű������� ���� �־ return ���� �̹� 100���� �����Ǿ� �����Ƿ� 100 �����
		System.out.println(a);
	}

}


