import java.util.Scanner;

abstract class Cal {
	protected int a, b; // private �� ��� �ȵǳ� protected�� ��� ����

	abstract int cal(); // �߻� �޼ҵ�

	void in() {  
// ���������� ������ ���� �� int �� ������ ��
		Scanner s = new Scanner(System.in);
		System.out.println("�� �� �Է�");
		int a = s.nextInt();
		int b = s.nextInt();
	}

	void pr() {
		in();
		int r = cal();
		System.out.println("����� " + r);
	}
}

class Multi extends Cal {

	int cal() {

		return a * b;
	}
}

class Div extends Cal {

	int cal() {

		return a / b;
	}
}

public class TT {  // �� ���
	public static void main(String[] args) {
		Multi m = new Multi();
		Div d = new Div();
		m.pr();
		d.pr();

	}
}


