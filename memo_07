public class TT0629TT {

	static void a(double b) {
		System.out.println("���� ���̴�: " + 3.14 * b * b);
		System.out.println("���� �ѷ���: " + 3.14 * b * 2);
	}

	public static void main(String[] args) {
		a(5.5);
	}

}

