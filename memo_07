[0731]

* ���̺� ���� �� �������� ���� ����:

create table t_enrol(sub_no char(3), stu_no number(9), emr_grade number(3),

constraint enr_sub_no_fk1 
foreign key(sub_no) references subject(sub_no),

constraint enr_stu_no_fk1 
foreign key(stu_no) references student(stu_no),

constraint enr_pk1 
primary key(sub_no,stu_no));


-----------------------------

<���̺� ����/ �⺻Ű ����>

create table t_tbl(t_empno number(4) not null, t_ename varchar2(40), t_job varchar2(9), t_mgr number(4), 
t_hiredate date, t_sal number(7,2), t_comm number(7,2),
t_deptno number(2), 
constraint t_tbl_pk primary key(t_empno));


* ���� �� �߰�
alter table t_tbl add(t_gender char(1));

* ���� �� Ÿ�� ����
alter table t_tbl modify(t_gender varchar2(10));

* ���� �� ����(�ý��� ������ �����Ұ�)
alter table t_tbl drop (t_gender);

* ���̺� �� ��� ������ ����
truncate table t_tbl; --�ѹ� �ȵ�!(delete�� �ѹ� ����)




