import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.event.ItemEvent;
import java.awt.event.ItemListener;

import javax.swing.JCheckBox;
import javax.swing.JFrame;
import javax.swing.JLabel;



public class TT extends JFrame {
	JCheckBox[] cafe=new JCheckBox[3]; // ��ü�迭(üũ�ڽ��迭)
	String[]menu= {"��","�Ƹ޸�ī��","�׸�Ƽ"};
	JLabel total; // ���� ���(��ŭ �Ծ�����)
	Check ch=new Check();
	
	Container c=getContentPane();
	TT() {
			setSize(500,500);
			setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
			setLayout(new FlowLayout());
		
			Check ch=new Check();
			for(int i=0;i<cafe.length;i++) {
				// cafe �迭�� ���� ����
				cafe[i]=new JCheckBox(menu[i]);
				c.add(cafe[i]);
				cafe[i].addItemListener(ch);
			}
			
			total=new JLabel();
			c.add(total);
			setVisible(true);
	}
	
	class Check implements ItemListener{
		int sum=0;
		public void itemStateChanged(ItemEvent e) {
			if(e.getStateChange()==ItemEvent.SELECTED) { // ���°� ���ߴ��� Ȯ��
				if(e.getItem()==cafe[0]) { //� ���������� Ȯ��
					sum+=5000;
				}
				else if(e.getItem()==cafe[1]) {
					sum+=4000;
				}
				else {
					sum+=4500;
				}
			}
			total.setText("����"+sum+"��");
		}
	}
	public static void main(String[] args) {
		new TT();
	}
}


