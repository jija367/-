class Car {
	private String color;
	private int speed;

	Car() {  // ������
		color = "��";
		speed = 0;
	}
	
	public String getColor() {  // �޼ҵ�
		return color;
	}
	public int getSpeed() { // �޼ҵ�
		return speed;
	}

}

public class TT {
	public static void main(String[] args) {
		Car c1=new Car();
		Car c2=new Car();
		System.out.println(c1.getColor()+" "+c1.getSpeed());
		System.out.println(c2.getColor()+" "+c2.getSpeed());
		
	}

}


