setPriority(), etPriority(), MAX_PRIORITY


class Th extends Thread{
	String str;
	Th(String str, int n){
		this.str=str;
		setPriority(n); // ������ �켱���� ����
	}
	public void run() {
	for(int i=0; i<10;i++) {
		System.out.println(str);
		System.err.println(getPriority()); // �켱���� ���� ��
	}
	}
}

public class PP {
	public static void main(String[] args) {
		Th t1=new Th("ȫ�浿",Thread.MAX_PRIORITY); // �ִ�
		Th t2=new Th("���浿",Thread.MIN_PRIORITY); //����
		Th t3=new Th("�̱浿",Thread.NORM_PRIORITY); // ��� �켱����
	t1.start();
	t2.start();
	t3.start();
	}
}


