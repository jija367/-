	TT(){
		setSize(500,500);	
		JMenuBar j=new JMenuBar();  // 1�� �޴��� ����
		JMenu j1=new JMenu("File");
		JMenu j2=new JMenu("Edit");
		JMenu j3=new JMenu("Source"); // 2�� �޴� ����
		JMenuItem j4=new JMenuItem("New");
		JMenuItem j5=new JMenuItem("Undo Typing"); // 3�� �޴��� ����
		setJMenuBar(j); // ȭ�鿡 �޴��� ����
		j.add(j1); // �޴��ٿ� �޴� ����
		j.add(j2);
		j.add(j3);
		j1.add(j4); // �޴��ȿ� ���� ����
		j1.add(j5);
		setVisible(true);
	}
