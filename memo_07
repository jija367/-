import java.util.Vector;
class Xy{
	private int x,y;
	Xy(int x, int y){
		this.x=x; this.y=y;
	}
	public String toString() {
		return x+" "+y;
	}
}
public class GGG {
	public static void main(String[] args) {
		// <> �ȿ� Ŭ�������� ��. �ڷ����� �ȵ�
		Vector<Xy> v=new Vector<Xy>();
		v.add(new Xy(5,6));
		v.add(new Xy(3,6));
		v.add(new Xy(4,6));
		v.remove(2);
		// remove() -> ��ȣ �ȿ��� �ε�����. ��ü ����-> 
		//v.add(new Xy(4,6)); ������
		for(int i=0;i<v.size();i++) {
			System.out.println(v.get(i));
		}
		
	}
}



