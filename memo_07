�������̽� ����

interface Shape{
	double pi=3.14;
	default void sh() {
		System.out.println("����");
	}
	abstract double area();
}
class SS implements Shape{
	double a;
	SS(double a){
		this.a=a;
	}
	public double area() {
		return a*a*pi;
	}
}
public class PPP {
	public static void main(String[] args) {
		
		SS s=new SS(3.1);
		System.out.println(s.area());
		s.sh();
		
	}
}


