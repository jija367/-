[�������̵�, final ����]

class Car {
	public int gas;

	public void up() {
		gas += 1;

	}

	public final void stop() { // final
		System.out.println("stop");
		gas = 0;
	}
}

class Sportscar extends Car {
	public void up() { // �������� public �� ���̸� �Ʒ��� ���� �ٿ���� ��
		gas += 100;
		// ����Ŭ������ �޼����� up�� �̸��� ����-> ������ �Ͽ� ���� ���� �ο���. �������̵�
	}
	/*
	 * public void stop() { // final �� �پ��� ������ �������̵� �Ұ�, ����
	 * System.out.println("������ī"); gas=0; }
	 */
}

public class TT0707 {
	public static void main(String[] args) {

		Car c = new Car();
		c.gas = 0;
		c.stop();
		
		Sportscar s=new Sportscar();
		
		s.stop(); // ����Ŭ�������� ��������(�����Ǵ� �ȵ�) �żҵ�
		
	}
}


