create or replace procedure pro(
v_empno in emp.empno%type,
v_ename in emp.ename%type,
v_job in emp.job%type,
v_mgr in emp.mgr%type,
v_sal in emp.sal%type,
v_deptno in emp.deptno%type)
is begin
insert into emp (empno, ename, job, mgr, sal, deptno)values(v_empno, v_ename, v_job, v_mgr, v_sal, v_deptno);
commit; -- Ŀ���� �־ �ǰ� �ȳ־ ��
end pro;
/


--emp ���̺� ������ Ȯ��
select * from emp;

--�� ����
execute pro(11,'RR','AA',3333,1111,33);



