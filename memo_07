--��ü �л����� Ű(����) ���� �ִ� �л� ��
select count(*), count(stu_height) from student;


-- ü���� 60 �̻��� �л��鸸 �����Ͽ� �а����� �׷�ȭ
select stu_dept, count(*) from student where stu_weight>=60 group by stu_dept;


--�ִ� Ű�� 175 �̻��� �а��� �а��� �ִ� Ű ���
select stu_dept, max(stu_height) from student group by stu_dept having max(stu_height)>=175;



-- �й�, �̸�, ����, ������ ���/ ������ ���ڸ� ������ -5kg, ���ڸ� +5kg

<case �̿�>
select stu_no, stu_name, stu_gender, stu_weight, case stu_gender when 'F' then stu_weight-5 
when 'M' then stu_weight+5 else stu_weight end as ����ȸ����� from student;


<decode �̿�>
select stu_no, stu_name, stu_gender, stu_weight, decode(stu_gender, 'F', stu_weight-5, 
'M', stu_weight+5, stu_weight)as ����ȸ����� from student;



-- �а��� �׷�ȭ�Ͽ� ���ü�� ����
select stu_dept, avg(stu_weight)as ���ü�� from student group by stu_dept;


--���� �л��� �� �г⺰ ���Ű�� 160 �̻��� �г�� ��� ���� ����
select stu_grade, avg(stu_height) from student where stu_dept='���' group by stu_grade having avg(stu_height)>=160;


--����ڹ�ȣ�� null�� ��� 'ceo'�� �ٲ� �����ȣ, �̸�, ����� �����ȣ�� �˻�
select empno, ename, nvl(to_char(mgr),'CEO') from emp; 


--��ǻ������ �а��� �������� �ٸ��а��� �л� ������ in�� �̿��Ͽ� ����
select * from student where stu_class in (select stu_class from student where stu_dept='��ǻ������')
and stu_dept<>'��ǻ������';


-- ����̸��� ���� ����
select count(ename)from emp;

--������̺��� Ʃ�� �� �˻�
select count(*) from emp;

-- �μ��� ������� �ο��� �˻�
select deptno, count(*) from emp group by deptno;

--����ڻ����ȣ�� ������� �ο� �� �˻�
select mgr, count(*) from emp group by mgr;


--ü���� 60 �̻��� �л��� �����ϴ� ������ �����̸��� ��� �˻�(equi join)
select sub_name from subject, student, enrol where subject.sub_no=enrol.sub_no and
student.stu_no=enrol.stu_no and stu_weight>=60;




