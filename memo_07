 // ���� ����ϳ� �Ű����� �Է� ���ϰ� �������� ����


public class TT0629TT {

	static void a() {
	
		int sum=0;
		int ary[][]= new int [2][2];
		for(int i=0;i<ary.length;i++) {
			for(int j=0;j<ary[i].length;j++) {
				ary[i][j]=(int)(Math.random()*10); 
				System.out.print(ary[i][j]+"\t");
				sum+=ary[i][j];
			}
			System.out.println();
		}
		System.out.println("������ "+sum);
	
	}

	public static void main(String[] args) {
		a();
	}

}

