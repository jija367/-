// ���� ���� ���Ҷ� length�ʵ�� �� �����̹Ƿ� �迭��[��].length�� �����ش�
public class TT {
	public static void main(String[] args) {
		double score[][] = { { 3.3, 2.3 }, { 4.4, 3.2 }, { 4.2, 5.3 } };
		double sum = 0.0;
		for (int i = 0; i < score.length; i++) {
			for (int j = 0; j < score[i].length; j++) {

				sum += score[i][j];
			}

		}
		System.out.println(sum);
		int a = score.length;
		int b = score[0].length;

	}

}


