// Ŭ���� ��ӿ���2



// Ŭ���� ��ӿ���

class XY {
	private int x, y;

	void set(int x, int y) { // ����. �Ű����� �ʱ�ȭ �۾�
		this.x = x;
		this.y = y;
	}

	void show() {
		System.out.println(x + " " + y);
	}
}

class XYZ extends XY{  // ��ӹ���(set(), show() �޼ҵ忡 ���ٰ���)
	// �ڽ� Ŭ���� ����:  �ڽĸ� extends �θ��
	private String name;
	void setname(String name) {
		this.name=name;
	}
	void pr() {
		System.out.println(name);
		show();  // �Լ�ȣ��=> ��������
	}
}

public class T0707 {
	public static void main(String[] args) {
		XY x=new XY();  // �����ڰ� ���� () �ȿ� �� ���� �� ����
		// �Ű������� private �̱� ������ ���� ���� �Ұ�
		x.set(1,2); // �Լ��� ���� ���� �����Ͽ� �ʵ忡 �� �Է�
		x.show(); // �Է� �� �� ���
		
		XYZ y=new XYZ();
		y.set(2, 5);  // set �޼ҵ� ����
		y.show();  // set�޼ҵ忡 �Էµ� ������ show �޼ҵ� �����Ͽ� ���
		y.setname("ddd");  // �ڽ�Ŭ������ ������ setname �޼ҵ忡 �� �Է�
		y.pr(); // �ڽ� Ŭ������ ������ �޼ҵ�� ���
		


