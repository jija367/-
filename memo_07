���� 6


interface Re {
	abstract public int area();
	default public void show() {

		System.out.println("�簢��!");
	}
}

class Rec implements Re{
	int a,b;
	Rec(int a, int b){
		this.a=a;
		this.b=b;
	}
	public int area() {
		return a*b;
	}
	
}
public class TT {
	public static void main(String[] args) {
	
		Re r=new Rec(10,20);
		r.show();
		System.out.println("����"+ r.area());
	}

}



