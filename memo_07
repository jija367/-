ȥ�� ����2 ��ü�迭

import java.awt.Color;
import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.Frame;
import java.util.Scanner;

import javax.swing.JButton;
import javax.swing.JFrame;


public class PP {
	public static void main(String[] args) {
		JFrame j=new JFrame();
		j.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		Container c=j.getContentPane();
		c.setBackground(Color.yellow);
		j.setTitle("GUI����");
		j.setSize(500,300);
		
		JButton jj[]=new JButton[5];
		Scanner s=new Scanner(System.in);
		for(int i=0;i<jj.length;i++) {
			System.out.println("��ư �̸�");
			jj[i]=new JButton(s.next());
			j.add(jj[i]);
			jj[i].setBackground(Color.red);
		}
		
		j.setLayout(new FlowLayout());
		
		j.setVisible(true);
	}
}

[0714]

[������]
synchronized
wait-notify





class Cook { // �丮 ������
	String food;
	boolean send = false; // ���� ���� �ȵ�

	void set(String f) {
		food = f; // �ʱ�ȭ
		send = true; // �������޿Ϸ�
		// synchronized �޼ҵ� �տ� Ű���� �ִ¹���� ������ �����ϴ� ��� �ΰ��� ����
		synchronized (this) {
			notifyAll(); // ���ڰ� �ִ� ������ �� �����
			// notify -> �ϳ��� �����常 ����, All�� ��� ������ ����
		}
		
	}
	
	String get() {
		if(send==false) {  // ������ �ϼ� �ȵǸ� ��ٸ�
			try {
				synchronized(this) {
					wait();
				}
			}catch(Exception e) {e.printStackTrace();}
		}
		return food;
	}
}

class Chef extends Thread{ // �丮�� ������
	Cook c;
	Chef(Cook c){
		this.c=c;
	}
	public void run() {
			c.set("����");  // ������ ���� �մԿ��� �������� ��Ȳ
		}
}
class Costumer extends Thread {
	Cook c;
	Costumer(Cook c){
		this.c=c;
	}
	public void run() {
	System.out.println(c.get()); // ������ ��� ��Ȳ
			}
}

public class TT {
	public static void main(String[] args) {
		Cook co=new Cook();
		Costumer c1=new Costumer(co);
		Costumer c2=new Costumer(co);
		Chef c3= new Chef(co);
		try {
			c1.start();
			c2.start();
			Thread.sleep(1000);
			c3.start();
			
			c1.join();
			c2.join();
			c3.join();
		}catch(Exception e) {}
	}
}

