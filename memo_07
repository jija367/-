import java.awt.BorderLayout;
import java.awt.Color;
import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.Panel;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
import java.awt.event.KeyAdapter;
import java.awt.event.KeyEvent;
import java.awt.event.KeyListener;

import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JComboBox;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JOptionPane;
import javax.swing.JTextField;

public class GGG extends JFrame {
	GGG(){
		setTitle("���̾�α�");
		setSize(500,500);
		Container c=getContentPane();
		c.add(new Dia(),BorderLayout.NORTH);  // ��ġ ����
		setVisible(true);
	}
	class Dia extends Panel{
		JButton b=new JButton("�̸��Է�");
		JButton b1=new JButton("Ȯ��");
		JButton b2=new JButton("�޽���");
		JTextField j1=new JTextField(10);
		Dia(){
			setBackground(Color.cyan); 
			// panel �� ��ӹ޾Ƽ� ���� �����̳� ���� �ʿ� ����
			add(b); add(b1); add(b2); add(j1);
			
			b.addActionListener(new ActionListener() {
				public void actionPerformed(ActionEvent e) {
					String name=JOptionPane.showInputDialog("�̸��Է�");
					// �˾�â�� �Է��� �̸����� name�� ����
					if(name!=null) { // �̸��Է��� ������(�Է����� ������ null)
						j1.setText(name);
					}
				}
			});
			
			b1.addActionListener(new ActionListener() {
				public void actionPerformed(ActionEvent e) {
					int r=JOptionPane.showConfirmDialog 
							// Ȯ�δ��̾�α״� int ��
					(null, "����Ұ̴ϱ�?","Ȯ��", JOptionPane.YES_NO_CANCEL_OPTION);
					if(r==JOptionPane.CLOSED_OPTION) {
						// �Է� ���� �ݾƹ����� ��
						j1.setText("���þ���");
					}
					else if(r==JOptionPane.YES_OPTION) {
						j1.setText("��");
					}
					else {
						j1.setText("�ƴϿ�");
						
					}
					
				}
			});
			
			b2.addActionListener(new ActionListener() {
				public void actionPerformed(ActionEvent e) { 
					//�޽������̾�α�
					JOptionPane.showMessageDialog
					(null, "���","�޽���",JOptionPane.ERROR_MESSAGE);
				}
			});
		}
		
	}
	public static void main(String[] args) {
		new GGG();
	}

}



