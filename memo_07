class Java{
	String title;
	Java(String title){
		this.title=title;
	}
	void show() {
		System.out.println(title);
	}
}
class DB{
	int n;
	DB(int n){
		this.n=n;
	}
	void pr() {
		System.out.println(n);
	}
}

class Program<T>{  // ������ <> : ���׸� ��ȣ
	T t; 
	// int a; ���� ��������. � Ŭ�������� ���������� �ʾ� ��� Ŭ������ �͵� ��� ���ٴ� ��
	// t�� ��ü
	void in(T t) { // � Ŭ������ �͵� ������ٴ� ��
		this.t=t;
	}
	T get(){
		return t; // ��ȯ���� Ŭ������ ��ü
	}
}
public class GGG {

	public static void main(String[] args) {
		
		Program<Java> p=new Program<Java>(); //Java Ŭ������ �־ ��üȭ
		p.in(new Java("sss")); 
		// Java j=new Java("sss"); j��� ��ü�� ����� �־ ��.
		new Java("sss").show();
		
		
		Program<DB> d=new Program<DB>();
		d.in(new DB(10));
		DB dd=d.get();  // ���׸����� ���Ϲ��� ��ü�� DB�� ����
		dd.pr();
		
		// Program<Integer> ����Ŭ������ �̿밡��
	}

}


