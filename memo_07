	int size;
	Ipad(String c, int n, int s){
		super(c,n);
		size=s;
	}
	void show() {
		
		System.out.println(color+"�� "+number+"�� "+size+"��ġ");
//���� ����� ����ʵ带 ȣ���ؾ� ��

	}
