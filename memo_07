class AAA{
	public void ride() {  // �������̵�
// public final void ride() / final �� ���̸� �������̵��� ���ϰ� ����
// ���������� ��� ����
		System.out.println("�������̵�aa");
	}
	public void load() {
		System.out.println("�����ε�aa");
	}
	
}

class BB extends AAA{
	public void ride() {  
		System.out.println("�������̵�bb");  // �θ�Ŭ�������� ��ӹ��� �Լ��� ������
	}
	public void load(int n) { // �Լ��̸��� �Ȱ����� �Ű��������� �ٸ�- �����ε�
		System.out.println("�����ε�bb");
	}
}

public class BBB {
public static void main(String[] args) {
	
	AAA a=new AAA();
	a.ride();
	a.load();
	BB b=new BB();
	b.ride();
	b.load();
	// load() �ȿ� int ���� ���� ������ �ڽ�Ŭ������ �ƴ� �θ� Ŭ���� �Լ��� ȣ���(�Ű������� �޶�)
