import java.util.Scanner;

public class P0625 {
	
	static void func(int a) {
		System.out.println("�� "+a/5+" ������ "+a%5);
	}

	
	static void pr(int ary[], int a) {
		int c[]=new int[5];
		for(int i=0;i<ary.length;i++) {
		c[i]=ary[i]/a;
		for(int j=0;j<c[i];j++) {
			System.out.print('#');
		}
		System.out.println();
		}
	}
	
	static char show(char a, int b) {
		for(int i=0;i<b;i++) {
			System.out.print(a);
					}
		return a;
	}
	
	public static void main(String[] args) {
		int aa[] = new int[10];
		System.out.println("����1");
		Scanner s = new Scanner(System.in);
		for (int i = 0; i < 10; i++) {
			aa[i] = s.nextInt();
			if (aa[i] % 4 == 0) {
				System.out.println(aa[i]);
			}
		}

		System.out.println("����2");
		int a = 50, b = 150;
		System.out.println(a > b ? a * b : a % b);

		System.out.println("����3");
		int ii = 100;
		while (ii >= 0) {
			if (ii % 2 == 0 && ii % 7 == 0) {
				System.out.print(ii + " ");
			}
			ii--;
		}
		System.out.println();
		System.out.println("����4");
		try {
			System.out.println("������ �Է��ض�");
			int aaa = s.nextInt();
			if (aaa % 2 == 0) {
				System.out.println("¦��");
			} else {
				System.out.println("Ȧ��");
			}
		} catch (Exception e) {
			System.out.println("������ �ƴ�");
		}

		System.out.println("����5");

		while (true) {
			System.out.println("�� ������ �Է�");
		try {
				int bb = s.nextInt();
				int cc = s.nextInt();
				System.out.println("����: " + bb + cc);
				break;
			
		} catch (Exception e) {
			System.out.println("�Ǽ��� �ȵ�");
			s.nextLine();  
			//(�Է� ��Ʈ���� �ִ� ������ �ƴ� ��ū�� ������ �ڵ�)
			// �߸� �Է��� ���� ������
			// nextLine�� ���Ͱ����� ������� ���� ��������
			// ���� ���� �� �Է��� �� ������ ����
			continue;
		}
						
			
		}
			
		
			/*while (true) {
				System.out.println("�� ������ �Է�");
