import java.io.BufferedReader;
import java.io.ByteArrayInputStream;
import java.io.ByteArrayOutputStream;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.FileReader;
import java.io.IOException;
import java.util.Scanner;

public class TT {
	
	static int same (int a[], int b[]) {
		int c=0;
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<b.length;j++) {
			if(a[i]==b[j]) {
				c++;
				}
			}
		}
		return c;
	}
	
	static void show(int a[]) {
		for(int i=0;i<a.length;i++) {
			
			System.out.print(a[4-i]+" ");
		}
	}

	public static void main(String[] args) throws IOException {
		
		System.out.println("����1");
		byte[] b= {4,6,7,8,-1,24};
		FileOutputStream out=new FileOutputStream("test.txt");
		ByteArrayInputStream in=new ByteArrayInputStream(b);
		int r;
		while((r=in.read())!=-1) {
			out.write(r);
		}
		in.close();
		out.close();
		
		System.out.println("����1.2");
		FileInputStream inn=new FileInputStream("test.txt");
		ByteArrayOutputStream outt=new ByteArrayOutputStream();
		
		int i;
		for(i=0; (i=inn.read())!=-1;i++) {
			outt.write(i);
		}
		inn.close();
		outt.close();
		byte a[]=outt.toByteArray();
		for(int ii=0;ii<a.length;ii++) {
		System.out.print(a[ii]+" ");}
		
		System.out.println();
		System.out.println("����2");
		
		int lotto[]= {4,10,25,30,45,47};
		int my[]= {1,4,7,26,45,48};
		int r1;
		r1=same(lotto,my);
		System.out.printf("��ġ�ϴ� ��ȣ�� ����: %d\n",r1);
		
		
		System.out.println("����4");
		int ary[]= {6,2,8,4,9};
		
		show(ary);
		
		System.out.println();
		System.out.println("���� 5");
		
		int ary1[][]= {{43,97},{34,77,87},{100,95,38,89}};
		int sum=0;
		int count=0;
		for(int i2=0;i2<ary1.length;i2++) {
			for(int j1=0;j1<ary1[i2].length;j1++) {
				sum+=ary1[i2][j1];
				count++;
			}
		}
		
		
	System.out.println("����: "+ sum);
	System.out.printf("���: %.1f\n",(double)sum/count);
		
	
	/*System.out.println("����6");
