[�̸�-���̵� ���߱� ����]


import java.util.Random;
import java.util.Scanner;
import java.util.Vector;

class Person{
	private String name;
	private int id;
	Person(String name, int id){
		this.name=name; this.id=id;
	}
	int getid() {
		return id;
	}
	String getname() {
		return name;
	}
	public boolean equals(Object obj) {
		Person p=(Person) obj;
		if(p.getid()==this.id&&p.getname().equals(this.name)) {
			return true;
		}
		return false;
	}	
}

class Game{
	Scanner s=new Scanner(System.in);
	Vector<Person> v=new Vector<>();
	Game(){ // �ʱ�ȭ
		v.add(new Person("dd",123));
		v.add(new Person("ss",456));
		v.add(new Person("ff",789));
		v.add(new Person("aa",112));
	}
	void in() {
		while(true) {
			System.out.println("�̸�, id");
			String name=s.next();
			if(name.equals("stop")) {
				break;
			}
			int id=s.nextInt();
			Person pp=new Person(name,id);
			if(v.contains(pp)) {
				System.out.println(pp.getname()+"�� �̹� ����");
			}
			else v.add(pp
					);
		}
	}
	void out() {
		while(true) {
			Random r=new Random();
			int n=r.nextInt(v.size());
			Person p=v.get(n);
			String str=p.getname();
			System.out.println(str+" id ��?");
			int id=p.getid();
			Scanner s=new Scanner(System.in);
			int i=s.nextInt();
			if(i==-1) {
				break;
			}
			if(i==p.getid()) {
				System.out.println("����");
			}
			else {
				System.out.println("����");
			}
		}
	}
	void end() {
		System.out.println("����");
		System.exit(0); // ���α׷� ����
	}
}
public class GGG {	
	public static void main(String[] args)  {
		Game g=new Game();
		Scanner s=new Scanner(System.in);
		while(true) {
			System.out.println("1,2,3");
			int a=s.nextInt();
			switch (a) {
			case 1: g.in();
				break;
			case 2: g.out();
				break;
			case 3: g.end();
				break;
			default:
			}
		}
	}
}


