[object Ŭ������ �޼ҵ�]
boolean : equals(Object obj)
String Ŭ������ �ִ� equals�� �ٸ�. ������ �ٸ��� ��ü ��
String : toString()  



// �ΰ��� ��ü ���ϴ� �Լ� ����
class In{
	int num;
	In(int num){
		this.num=num;
	}
	boolean eq(In i){ // �Ű������� Ŭ���� ��ü ���� ����
		if(this.num==i.num) {
			return true;
		}
		return false;
	}
}
public class TT {

	public static void main(String[] args) {
		
		In i1=new In(1);
		In i2=new In(5);
		In i3=new In(5);
		
		if(i1.eq(i2)) {
			System.out.println("i1,i2�� ����");
		}
		else {
			System.out.println("i1, i2�� �ٸ�");
		}
		if(i2.eq(i3)) {
			System.out.println("i2,i3�� ����");
		}
		else {
			System.out.println("i2, i3�� �ٸ�");
		}
		
	}

}


