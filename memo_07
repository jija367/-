<���̺� ���� ���� ����>


1. ���̺� ����

create table customers (c_code varchar2(4) constraint c_code_1 primary key, c_name varchar2(30),
c_ceo varchar2(12), c_addr varchar2(100), c_phone varchar2(13));

create table product (p_code char(3) constraint p_code_1 primary key, p_name varchar(30),
p_cost number, p_group varchar2(30));

create table stock(p_code char(3) constraint p_code_2 primary key, s_qty number, s_lastdate date);


2. ���̺� ���� ��ȸ

desc customers;
desc product;
desc stock;


