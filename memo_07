import java.util.HashSet;
import java.util.Iterator;

public class GGG {

	public static void main(String[] args) {
		HashSet<String> h=new HashSet<>();
		h.add("��ǻ��");
		h.add("���콺");
		h.add("��ǻ��");
		
		Iterator<String> i=h.iterator();
		while(i.hasNext()) {
			System.out.println(i.next());
		}
	}
}

