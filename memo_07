hashmap �α��� ����

import java.util.HashMap;
import java.util.Scanner;

public class GGG {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		HashMap h=new HashMap();
		h.put("id","123");
		h.put("id1","1234");
		h.put("id2","12345");
		
		while(true) {
			System.out.println("id/pw�Է�");
			String id=s.next();
			String pw=s.next();
			if(!h.containsKey(id)) { //id �� ���� ���
				System.out.println("�Է��� id �� �������� �ʽ��ϴ�");
				continue;
			}
			else { //id�� ���� ���
				if(!h.get(id).equals(pw)) { //����� ��ġ���� ���� ���
					System.out.println("��й�ȣ�� ��ġ���� �ʽ��ϴ�");
					continue;
				}
				else {
					System.out.println("�α���");
					break;
				}
			}
			
		}
	}
}



