import java.awt.FlowLayout;
import java.util.Vector;

import javax.swing.JComboBox;
import javax.swing.JFrame;

class My{
	String a; int b;
	My(String a, int b){
		this.a=a; this.b=b;
	}
	public String toString() {
		return a+" "+b;
	}
}
public class GGG {

	public static void main(String[] args) {
		JFrame j=new JFrame();
		j.setLayout(new FlowLayout());
		j.setSize(500,500);
		Vector<My> v=new Vector<>();
		v.add(new My("ss",20));
		v.add(new My("dd",23));
		v.add(new My("ff",22));
		JComboBox<My> c=new JComboBox<My>(v);
		j.add(c);
		
		j.setVisible(true);
	}
}

