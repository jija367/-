import java.awt.Color;
import java.awt.Container;
import java.awt.FlowLayout;
import java.awt.event.MouseEvent;
import java.awt.event.MouseMotionAdapter;

import javax.swing.JComboBox;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JSlider;
import javax.swing.event.ChangeEvent;
import javax.swing.event.ChangeListener;

public class GGG extends JFrame{
	JSlider j=new JSlider(100,200,150); // ���� �ּ�,�ִ�,������ġ�� ��
	JLabel j1=new JLabel(" ");
	
	GGG(){
		setSize(500,500);
		Container c=getContentPane();
		setLayout(new FlowLayout());
		add(j); add(j1);
		
		j.setMajorTickSpacing(20); // ū ���� ����
		j.setPaintLabels(true); 
		// ���� ���̺��� ǥ������ �����Ѵ�. ������ ���� �Ⱥ���
		j.setPaintTicks(true); //������ ǥ���� ���� �����Ѵ�
		
		j1.setHorizontalAlignment(JLabel.CENTER); 
		// ���ڿ� ��ġ�� �߾ӿ� ��ġ
		j1.setOpaque(true); // ������ ����(������ ������ �� �ȳ��ö�)
		j1.setBackground(Color.magenta);
		j1.setText(Integer.toString(j.getValue())); 
		//j.getValue()->�������� �� �������� ���ڿ��� ��ȯ�ؼ� ���
		//���簪�� 150���� �Ǿ� �־� ���� ���� 150�� �����
		
		j.addChangeListener(new ChangeListener() {
			//���� �̺�Ʈó��(���� �̵��� �߻�)
			public void stateChanged(ChangeEvent e) {
				JSlider s=(JSlider)e.getSource();
				j1.setText(Integer.toString(j.getValue()));
				//���� �̵��Ҷ����� ���� ���� ���ڿ��� ����
			}
		});
		setVisible(true);
	}
	public static void main(String[] args) {
		new GGG();
	}
}


