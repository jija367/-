import java.util.HashSet;
import java.util.Set;
class Stu{
	String a; int b;
	Stu(String a, int b){
		this.a=a; this.b=b;
	}
	
	public int hashCode() {
		return a.hashCode()+b;
		// a(���ڿ�)�� �ؽ��ڵ� ��+b
	}
	public boolean equals(Object obj) {
		if(obj instanceof Stu) {
			Stu n=(Stu)obj;
			return n.a.equals(a)&& n.b==b;
		}
		else {
		return false;
		}
	}
}
public class GGG {
	public static void main(String[] args) {
		Set<Stu> s=new HashSet<Stu>();
		s.add(new Stu("�뼮",24));
		s.add(new Stu("����",22));
		s.add(new Stu("����",22)); 
		System.out.println(s.size());
	}
}


