public class TT0629TT {
	static int a; // ��������(global variable)-�޼ҵ尡 �ƴ϶� Ŭ���� �ȿ� ����
	//���������� ��� �Լ����� ������ �ؾߵǱ� ������ static �� �̿�

	static void aa() {
		a = 10;  // ���� ����� �������� a�� ���� ���� ����� �������� a�� ������ 
	}

	static void bb() {
		int a; // ��������. ��������(static int a;)�� �̸��� ���� �ٸ� ����
		a = 20;
	}

	public static void main(String[] args) {

		aa();  // a=10
		bb();  // a=20
		System.out.println(a);  // main �� a�� ���� ���������� ����
		//bb�� �ִ� a���� bb �ȿ����� �̿밡���� ����
		//������ ��µǴ� a�� �������� a
	}

}

