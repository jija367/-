//������ ��ġ�� �ݵ�� BorderLayout�� �����ؾ� �Ѵ�
//��,��,��,��,�߾ӿ� ����





public class GGG extends JFrame{

	GGG(){
		setSize(500,500);
		//setLayout(new FlowLayout());
		Container c=getContentPane();
		JToolBar j=new JToolBar();
		JButton b=new JButton("��");
		c.add(j,BorderLayout.NORTH);
		j.add(b);
		b.addActionListener(new Jt());
		//���ٴ� �ݵ�� BorderLayout�� �����ؾ� �Ѵ�
		setVisible(true);
	}
class Jt implements ActionListener{
	public void actionPerformed(ActionEvent e) {
		int r=JOptionPane.showConfirmDialog
				(null,"���� ����?","����",JOptionPane.YES_NO_OPTION);
		if(r==JOptionPane.CLOSED_OPTION) { //������ ���� ���� ���
			return;
		}
		else if(r==JOptionPane.YES_OPTION) {
			System.exit(0);
		}
		else {
			return; // no����
		}
	}
}
	public static void main(String[] args) {
		new GGG();
	}
}


