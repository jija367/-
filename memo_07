public class TT {
	public static void main(String[] args) {
		int [] ary= {1,2,3,4,5};
		int sum=0;
		for(int aa:ary) {
			sum+=aa;
		}
		System.out.println(sum);
	}

}

---

[��ü �迭 �� �ʱ�ȭ/for each �� ����]




class Num {
	int n;

	Num(int n) {
		this.n = n;

	}

	int get() {
		return n;
	}
}

public class TT {
	public static void main(String[] args) {
		Num[] ary = new Num[] { new Num(1), new Num(2), new Num(3) };
		// ��ü �迭 �� �ʱ�ȭ
		int sum = 0;
		for (Num n : ary) { // for each�� ����, ary�迭�� �ִ� ��ü ���� n ��ü�� ����
			System.out.println(n.get());
			// ��ü�� ���� ����� �ȵǹǷ� �Լ��� �����ؼ� ���
			sum += n.get();

		}
		System.out.println(sum);
	}

}


