import java.util.Scanner;

public class TT0629TT {

	public static void main(String[] args) {
		int a[] = new int[7];
		int big=0;
		Scanner s = new Scanner(System.in);
		for (int i = 0; i < a.length; i++) {
			a[i] = s.nextInt();
			if(big<a[i]) {
				big=a[i];
			}
		}
		System.out.println(big);
	}

}



