������+Ŭ���� ȣ�� ����


class Cal {
	static int cnt = 0;

	int add(int a, int b) {
		cnt++;
		return a + b;
	}

	int sub(int a, int b) {
		cnt++;
		return a - b;
	}

	void show() {
		System.out.println("����Ƚ�� " + cnt);
	}
}

class Add extends Thread {
	public void run() {
		Cal c = new Cal();
		System.out.println(c.add(2, 3));
	}
}

class Sub extends Thread {

	public void run() {
		Cal c = new Cal();
		
		System.out.println(c.sub(4, 3));
	}
}

public class PP {
	public static void main(String[] args) {
		Cal c = new Cal();
		Add a = new Add();
		Sub s = new Sub();

		a.start();
		s.start();
		try {
			a.join();
			s.join();
		} catch (Exception e) {
		}
		c.show();
	}
}



<�������� ¥�� ��>

class Cal {
	static int cnt = 0;

	int add(int a, int b) {
		cnt++;
		return a + b;
	}

	int sub(int a, int b) {
		cnt++;
		return a - b;
	}

	void show() {
		System.out.println("����Ƚ�� " + cnt);
	}
}

class Add extends Thread {
	Cal c;

	Add(Cal c) {
		this.c = c;
	}

	public void run() {
		System.out.println(c.add(10, 3));
	}
}

class Sub extends Thread {
	Cal c;

	Sub(Cal c) {
		this.c = c;
	}

	public void run() {

		System.out.println(c.sub(10, 3));
	}
}

public class PP {
	public static void main(String[] args) {
		Cal c = new Cal();
		Add a = new Add(c);
		Sub s = new Sub(c);

		a.start();
		s.start();
		try {
			a.join();
			s.join();
		} catch (Exception e) {
		}
		c.show();
	}
}

