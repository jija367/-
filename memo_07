<����>

--sub_no�� 104���� ������ 5�� ���ҽ�Ű����
update enrol1 set enr_grade=enr_grade-5 where sub_no=104;


--���� �ֱٿ� �Ի��� 5���� �����ȣ �̸� �Ի��� �˻�(�Ի��� null ���� ����� �����ؾ� �ϹǷ� is not null �ʿ�)
select empno, ename, hiredate from 
(select empno, ename, hiredate from emp where hiredate is not null order by hiredate desc)
where rownum<=5;


--�μ��� ��� �޿��� ���� ū �μ� 2���� �μ��̸� ���ض�
select dname from (select dname, avg(sal) from emp, dept where dept.deptno=emp.deptno 
group by dname order by avg(sal) desc)
where rownum<=2;



