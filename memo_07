public class TT0629TT {

	static double area(double r) {
		return 3.14*r*r;  // ���Ͽ� �ƿ� ���� �Է� ����
	}
	static double round(double a) {
		return 3.14*a*2;
	}
	public static void main(String[] args) {
		System.out.println("���� ����: "+area(5.5));  // �Ű������� �������� �Է��ϸ� �ٷ� ����
		System.out.printf("���� �ѷ�: %.2f",round(3.2));
		//����: 3.14*������*������
		//�ѷ�: 3.14*������*2
	}

}



