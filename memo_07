// multi(2,10) 2�� 10�� �Լ� �����, ���ο��� ���

public class TT0629TT {
	static int multi(int a, int b) {
		
		int s=1;  // ������ ������ 1�� �����ؾ� ���� �� ����
		for(int i=1;i<=b;i++) {
			s*=a;
		}
		return s;
	}
	public static void main(String[] args) {
		System.out.println(multi(2,10));
	}

}

