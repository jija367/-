[�߻�Ŭ����]

��ü���� �Ұ���.
�Ϲ� �޼ҵ�, �ʵ� ������ ����
Ŭ���� �տ� abstract �ٿ���� ��
�߻� Ŭ���� �ȿ� �Ϲ� �޼ҵ�, �߻� �޼ҵ� �Ѵ� ����


abstract class Car{
	int speed=0;
	String color;
}


[�߻�޼ҵ�]

�޼ҵ� �տ� abstract �ٿ���� ��
�߻�޼ҵ�� �ݵ�� �߻� Ŭ���� �ȿ� �־�� ��. 
�� ������ �޼ҵ�� ��ӹ޴� ����Ŭ�������� �������̵� �ؼ� ���

abstract ��ȯ���޼ҵ� �̸�(�Ķ����);
abstract void upSpeed(int speed);




�߻�Ŭ���� ����

abstract class Car{  //�߻� Ŭ����
	String name;
	void run() { //�Ϲݸ޼ҵ�
		System.out.println("���� �����δ�");
	}
	abstract void stop();  // �߻�޼ҵ� {} �ٵ� �� �� ����. 
	// �߻� �޼ҵ�� �ݵ�� �߻�Ŭ���� �ȿ� �־�� ��
}

class Cars extends Car{ // ��ӹ��� �ڽ� Ŭ����
	Cars(){
		name="���Ǵ�Ƽ"; 
		// �θ�Ŭ������ �ʵ� name�� �����޾Ƽ� �̷��� �־ ��
		}
	void stop() { //�߻�޼ҵ�� �ݵ�� ��ӹ޾Ƽ� ����
		System.out.println("���� �����");
	}
}

public class TT {
	public static void main(String[] args) {
		
		Cars c=new Cars(); // Car�� �߻�Ŭ������ ��ü ���� �Ұ���
		c.stop();
	}

}


//��ü ������ �� ĳ���� �Ͽ�
// Car c=new Cars(); �̷��Ե� ���� ����

----

�߻�Ŭ���� ����

abstract class Ca{
	int a,b;
	
	
	abstract int add(int a, int b);
	abstract int avg(int a[]);
	// �߻�޼ҵ� ������ �Ű����� ������ ��ġ���Ѿ� ��
}
class Cal extends Ca{
	
	
	int add(int a, int b) {
		return a+b;
	}
	int avg(int a[]) {
		int sum=0;
		int c=0;
		for(int i=0;i<a.length;i++) {
			sum+=a[i];
			c++;
		}
		return sum/c;
	}
	


}

public class TT {
	public static void main(String[] args) {
		
		Cal c=new Cal();
		System.out.println(c.add(3,7));
		System.out.println(c.avg(new int[] {1,2,3}));
		// int a[]=new int[]{1,2,3} �� ����. �ʱ�ȭ�� ���� �״�� �־���
		
	}

}

---


[����ó�� ����/ super, Ŭ���� ��� �̿�]

import java.util.Scanner;

class Num extends Exception { // Exception �� �θ�Ŭ����
	Num() {
		super("�߸��� ��!!!"); // �θ�Ŭ������ ������ ȣ��
	}
}

public class TT {
	static int in() throws Num {
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		if(a<0) {
			Num num=new Num();  // ������ ȣ��
			throw num; // ����ó��
		}
		return a;
	}

	public static void main(String[] args) {
		
		System.out.println("����� �Է��ض�");
		try {
			int n=in();
			System.out.println(n);
		}catch(Exception e) {
			System.out.println(e.getMessage());
			// Num Ŭ�������� ����Ŭ������ Exception Ŭ������ ������
			// �ű⿡�� ���� �����޽��� ���
		}
		
	}

}


