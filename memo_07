���׸� ����

class Cont<T> {
	T t;

	void set(T t) {
		this.t = t;
	}

	T get() {
		return t;
	}
}

public class GGG {
	public static void main(String[] args) {
		Cont<String> c1 = new Cont<String>();
		c1.set("����");
		String s = c1.get();
		System.out.println(s);

		Cont<Integer> c2 = new Cont<Integer>();
		c2.set(5);
		int s2 = c2.get();
		System.out.println(s2);
	}
}


