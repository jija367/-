public class TT0629TT {
	static void pr(int id, double ki) {
		System.out.println(id);
		System.out.println("Ű��" + ki);

	}

	static void show() {
		System.out.println("�׷� �̸�..");
	}

	public static void main(String[] args) {
		double k=164.5;
		pr(123,156.6);
		pr(125,k);
		show();
		
		// k�� ��������
		//(�ش� ���� �ȿ����� ��ȿ�� �۾�, pr�Լ��� �ִ� ������ �̸��� ���� �ص� ������ �ٸ��� �ٸ���)
	}

}



