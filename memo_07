import java.util.Scanner;

class Song {
	String singer;
	String title;
	Song(String singer, String title){
		this.title=title;
		this.singer=singer;
		}
}

public class PP {
public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
	
	Song ss[]=new Song[2];
	for(int i=0;i<ss.length;i++) {
		System.out.println("����");
		String singer=s.next();
		System.out.println("����");
		String title=s.next();
		ss[i]=new Song(singer,title);
		System.out.println(ss[i].singer+" "+ss[i].title);
	}
	
}
	
}

