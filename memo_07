import java.util.Scanner;

enum Pro{ // ������
	JAVA(20),C(10),PYTHON(15);
	//JAVA��� ���ڿ��� 20�̶�� �����ε� ���ڴٴ� �ǹ�
	int n;
	Pro(int n){
		this.n=n;
	}
	int get() {
		return n;
	}
}

public class TT {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.println("��������?");
		String n=s.next();
		
		Pro p=Pro.valueOf(Pro.class,n);
		//valueOf ��ü��ȯ
		
		switch (p) {
		case JAVA: {
			System.out.println(p.n);
		}
		break;
		case C: {
			System.out.println(p.n);
		}
		break;
		case PYTHON: {
			System.out.println(p.n);
		}
		break;
		default:
		}
	}
}


