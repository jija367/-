�߻� Ŭ���� Ȧ¦ ���߱� ����

abstract class A {
	protected int n;

	A(int n) {
		this.n = n;
	}

	abstract boolean even();
}

class C extends A {
	C(int n) {
		super(n);
	}

	boolean even() {
		if (n % 2 == 0) {
			return true;
		}
		return false;
	}
}

public class PP {
	public static void main(String[] args) {
		C c = new C(13);
		System.out.println(c.even());

	}
}


