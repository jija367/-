		try {
			FileInputStream fii=new FileInputStream("h.txt");
			in=new DataInputStream(fii);
			int r=in.readInt();
			double g=in.readDouble();
			boolean h=in.readBoolean();
			// ���Ͽ� ������ �⺻ �ڷ��� �� �о��
			System.out.println(r);
			System.out.println(g);
			System.out.println(h);
			
		} catch (Exception e) {
		}
		in.close();
		
		/*DataOutputStream out=null;  // �⺻ �ڷ��� ���� ������ �� �ִ�
