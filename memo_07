// �� �� �� �� ū�� ���ϴ� �Լ� ¥��(return �޴� �Լ�)

public class TT {

	static double big(double a, double b) {
		double c;
		if(a<b) {
			c=b;
			System.out.println(c);
		}
		else
		{c=a;
			System.out.println(c);
		}
	return c;
	}
	public static void main(String[] args) {
		
		System.out.println(big(5.5,6.5));
		
		
	}

}



<������� § �ڵ�: 3�׿���>

public class TT {

	static double big(double a, double b) {
		return a>b?a:b;
	}

	public static void main(String[] args) {

		System.out.println(big(5.5, 6.5));

	}

}


