class Circle{
	private int r;
	Circle(int r){
		this.r=r;
	}
	public int get() {
		return r;
	}
}
class NCircle extends Circle{
	String color;
	NCircle(int r, String color){
		super(r); 
		// Circle�� �Ű������� ���� ���� ���°� �ƴ϶� 
		//Circle(int r)�����ڸ� ȣ���ѰŶ� ���� ȣ�Ⱑ��
		//�����ڿ� private �� ������ ����
		this.color=color;
	}
	public void show() {
		System.out.println(get()+" "+color);
	}
}
public class TT {
	public static void main(String[] args) {
		
		
		NCircle n=new NCircle(10,"red");
		n.show();  // ������10, red��
	}

}

