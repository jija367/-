public class GGG {

	public static void main(String[] args) {
		HashSet<Num> h=new HashSet<>();
		h.add(new Num(30));
		h.add(new Num(40));
		h.add(new Num(60));
		h.add(new Num(60)); 
		// set�� �ߺ������ ���� ������ ���� set�� �� ���� 
		//�����Ͱ� �ƴ϶� ��ü�̱� ������ ���ϰ� ��µȰ�
		
