<���׸�/ ��� ����>

class Food{
	void show() {
		System.out.println("����");
	}
}
class F extends Food{
	void show() {
		super.show();
		System.out.println("����");
	}
}
class Fo<T>{
	T t;
	void in(T t) {
		this.t=t;
	}
	T out() {
		return t;
	}
}
public class GGG {
	static void pr(Fo<? extends Food> f) {  
		// ?�� ������ Ŭ���� �����ϴ� �۾� -> Food�� ���� ��ӹ��� �͸� �� �� ����
		// Fo<? super Food> f -> Food�� �θ�
		Food fo=f.out(); // Food ��ü�� ���׸����� ���� ���� ��ü ���� �Է�
		fo.show(); // show �޼ҵ� ���
	}
	public static void main(String[] args) {
		Fo<Food>f=new Fo<Food>(); // ���׸� ��ü ����
		f.in(new F()); // ���׸� ��ü�� F��ü �Է�
		pr(f); // F Ŭ���� ���Ϲ޾� FŬ������ show �Լ��� �����
	}
}



