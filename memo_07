// str('*',10); �� ���� ����(while �� �̿�)
public class TT0629TT {
	static void str(char a, int b) {
		int i=0;
		while(i<b) {
			i++;
			System.out.print(a);
		}
	}

	public static void main(String[] args) {
		str('*',10);
		
	}

}


