--1.	����ڻ����ȣ�� 7698 �Ǵ� 7839�� ����� �����ȣ�� ����̸��� �˻��϶�.
select empno, ename from emp where mgr=7698 or mgr=7839;

--2.	��������� ��MANAGER�� �Ǵ� ��SALESMAN���� ����� �����ȣ, ����̸�, �μ���ȣ�� �˻��϶�.
select empno, ename, deptno from emp where job='MANAGER' or job='SALESMAN';

--3.	������� �����ȣ�� ����̸��� �����ȣ ������ �˻��϶�.
select empno, ename from emp order by empno;

--4.	������� �����ȣ�� ����̸��� �μ���ȣ�� �̸������� �˻��϶�.
select empno, ename from emp order by deptno, ename;

--5.	������� ������ �μ��� �޿��� ���� ������ �˻��϶�.
select * from emp order by deptno, sal desc;

--6.	������� ������ ��������� �޿� ������ �˻��϶�.
select * from emp order by job, sal;

--7.	������� ������ �μ���ȣ��, ���������, �޿� ������ �˻��϶�.
select * from emp order by deptno, job, sal;

--8.	������� ����̸��� �ҹ��ڷ� �˻��϶�.
select lower(ename) from emp;

--9.	������� ����̸�, ��������� �ҹ��ڷ� �˻��϶�.
select lower(ename), lower(job) from emp;

--10.	������� ����̸��� �빮�ڷ� �˻��϶�.
select upper(ename) from emp;

--11.	������� ����̸�, ��������� �빮�ڷ� �˻��϶�.
select upper(ename), upper(job) from emp;

--12.	������� ����̸��� ù�ڸ� �빮�ڷ� �˻��϶�.
select initcap(ename) from emp;

--13.	������� ����̸��� ��������� ù �ڸ� �빮�ڷ� �˻��϶�.
select initcap(ename), initcap(job) from emp;

--14.	������� ����̸��� ��������� �����Ͽ� �˻��϶�.(concat�̿�)
select concat(ename, job) from emp;

--15.	������� ����̸��� ����̸��� ù �α��ڸ� �˻��϶�.
select ename, substr(ename, 1,2) from emp;

--16.	������� ����̸�, ������� �׸��� ��������� �ι�°���� �����ڸ� �˻��϶�.
select ename, job, substr(job,2,3) from emp;

--17.	������� ����̸��� ����̸��� ���̸� �˻��϶�.
select ename, length(ename) from emp;

--18.	������� ����̸��� ��������� �ڸ����� �˻��϶�.
select ename, length(job) from emp;

--19.	������� ����̸��� ��A���� ��� ° ��ġ�� �ִ��� �˻��϶�.
select ename, instr(ename, 'A') from emp;

--20. ��������� ��A���� ��� ° ��ġ�� �ִ��� �˻��϶�.
select job, instr(job, 'A') from emp;

?
<�߰����� >


--21. ����̸��� 15�ڸ��� �ϰ�, �ڿ� ��&����ä�� �˻��϶�.

select rpad(ename,15,'&') from emp;


--22. ��������� 20�ڸ��� �ϰ�, �տ� ��%����ä�� �˻��϶�.

select lpad(job,20,'%')from emp;


--23. ����� �����ȣ, ����̸�, �޿��� �˻��϶�.(�޿��µι�° �ڸ����� �ݿø���)

select empno, ename, round(sal,-2) from emp;


--24. ����� �����ȣ, ����̸�, �޿��� �˻��϶�.(�޿��µι�° �ڸ����� ������)

select empno, ename, trunc(sal,-2) from emp;


--25. �����ȣ�ͱ޿��� 100���� ���� �������� �˻��϶�.

select empno, mod(sal,100) from emp;


--26. �����ȣ, ����̸�, �Ի���, �Ի��� 100���� ��¥�� �˻��϶�.

select empno, ename, hiredate, hiredate+100 from emp;


--27. �����ȣ, ����̸�, �Ի���, �ٹ� ���ڸ� ����Ͽ� �˻��϶�.

select empno, ename, hiredate, trunc(sysdate-hiredate,0) from emp;


--28. ������� �Ի��Ͽ��� 3��° �Ǵ� ��¥�� �˻��϶�.

select add_months(hiredate, 3) from emp;


--29. ������� �Ի��ϴ��� ������� ��¥�� �˻��϶�.

select hiredate, next_day(hiredate,7) from emp;


--30. ������� �Ի���Ǹ����� ��¥�� �˻��϶�.

select hiredate, last_day(hiredate) from emp;

?

--31. �μ��� ���������� �޿��� �˻��϶�.
select avg(sal) from emp group by deptno;

--32. �μ��� ������Ǳ޿��� ǥ�������� �˻��϶�.
select stddev(sal) from emp group by deptno;

--33. �μ��� �������������� �޿� ���� �˻��϶�.
select deptno, job, sum(sal) from emp group by deptno, job;

--34. �μ��� ���������������� ��� �޿��� �˻��϶�.
select deptno, job, avg(sal) from emp group by deptno, job;

--35. �μ��� ���������������� �Ի����� MAX�� MIN���� �˻��϶�.
select deptno, job, max(hiredate), min(hiredate) from emp group by deptno, job;

--36. �μ��� ��������ο����� �ο����� ���� ������ �˻��϶�.
select deptno, count(*) from emp group by deptno order by count(*) desc;

--37. �μ��� ���������� �޿��� ��� �޿� ������ �˻��϶�.
select avg(sal) from emp group by deptno order by avg(sal);

--38. �μ���, ��������� ����� �޿� ���� ���� ������ �˻��϶�.
select deptno, job, sum(sal) from emp group by deptno, job order by sum(sal) desc;

--39. �μ���, ��������� ������� ��� �޿��� ��� �޿��� ���� ������ �˻��϶�.
select deptno, job, avg(sal) from emp group by deptno, job order by avg(sal) desc;


--40. ������� �Ի����� ���� �������� �ݿø��Ͽ� �˻��϶�.
select round(hiredate, 'year') from emp;

--41. ������� �Ի����� ���� �������� �����Ͽ� �˻��϶�.
select trunc(hiredate,'year') from emp;

--42. ����ڻ����ȣ(mgr)�� ���� ����� ��� ������� �������� ��Ÿ������ �ϴ� ���ǹ��� �ۼ��ض�. 
select nvl(to_char(mgr), '����� ����') from emp;
 
--43. Ŀ�̼�(comm)�� ������ �޿��� �����ȣ, ����̸��� �Բ� �˻��϶�.
select empno, ename, nvl(comm,0)+sal as Ŀ�̼����Ա޿� from emp;

--44. Ŀ�̼��� ������ ������ �����ȣ, ����̸��� �Բ� �˻��϶�.
select empno, ename, (nvl(comm,0)+sal)*12 as Ŀ�̼����Կ��� from emp;


--45. 10, 20���μ� ����� �� �ְ� �޿��� �޴� ����� �����ȣ, ����̸�, �޿����˻��϶�. (������)
select deptno, empno, ename, sal from emp where sal in
(select max(sal) from emp group by deptno having deptno in(10,20));

--46. 30�� �μ��ǻ�� �� ���� �޿��� �޴� ����� �����ȣ, ����̸�, �޿����˻��϶�.(������)
select deptno, empno, ename, sal from emp where sal in
(select min(sal) from emp group by deptno having deptno=30);

--47. ��ü ������� �ְ� Ŀ�̼��� �޴� ����� �����ȣ, ����̸�, Ŀ�̼����˻��϶�.(������)
select empno, ename, comm from emp where comm=(select max(comm)from emp);

--48. ��ü ������� ���� Ŀ�̼��� �޴� ����� �����ȣ, ����̸�, Ŀ�̼����˻��϶�.(������)
select empno, ename, comm from emp where comm=(select min(comm) from emp);

--49. ��ü ����ǻ���̸� �� MAX�� MIN���� �˻��϶�.
select max(ename), min(ename) from emp;

--50. ��ü ������Ի��� �� MAX�� MIN���� �˻��϶�.
select max(hiredate), min(hiredate) from emp;

--51. ��� ���̺��� �μ� ���̺��� natural join�ض�.
select * from emp natural join dept;

--52. �����ȣ, ����̸�, �μ��̸��� �˻��϶�.(equi)
select empno, ename, dname from dept, emp where dept.deptno=emp.deptno;

--53. �����ȣ, ����̸�, �μ��̸��� �˻��϶�.(natural)
select empno, ename, dname from dept natural join emp;

--54. �����ȣ, ����̸�, �μ��̸��� �˻��϶�.(join~using)
select empno, ename, dname from dept join emp using(deptno);

--55. ������ NEW YORK�� ����̸��� �˻��϶�.(equi)
select ename from emp, dept where dept.deptno=emp.deptno and loc='NEW YORK';
