interface Po{
	
	void pr(String b);
	void pr(int a, String b);
}
class Point implements Po{
	
	public void pr(int a, String b) {
		System.out.println(b+"å�� "+a+"��");
	}
	public void pr(String b) {
		System.out.println(b);
	}
}
public class TT {
	public static void main(String[] args) {
		Po p=new Point();
		p.pr(30000,"java");
		p.pr("python");
	}
}

