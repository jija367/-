		JRadioButton j2=new JRadioButton("�ݶ�");
		JRadioButton j3=new JRadioButton("��Ƣ");
		this.add(j1);
		this.add(j2);
		this.add(j3);
		
		ButtonGroup gr=new ButtonGroup(); 
		// ������ư �ߺ����� �ʰ� �׷����� ������
		gr.add(j1);
		gr.add(j2);
		gr.add(j3);
		
