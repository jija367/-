// ��ü ���� �� �� 3�� �� �� for-each������ ����

class Num{
	int n;
	Num(int n){
		this.n=n;
	}
	int get(){
		return n;
	}
	
}
public class GGG {
	public static void main(String[] args) {
		Num n[]=new Num[3];
		int sum=0;
		for(int i=0;i<n.length;i++) {
			n[i]=new Num((int)(Math.random()*100)+1);
		}
		for(Num nn:n) {
			sum+=nn.get();
			System.out.print(nn.get()+" ");
		}
		System.out.println();
		System.out.println(sum);
		
	}
}


