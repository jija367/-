--���� ������ ã�� �Լ�(�������� ���ڷ� 2(�Ͽ���� ����)�̹Ƿ� 2�� �־ ��)
select next_day(sysdate,'MON') from dual;



***

��ȯ�Լ�

1. TO_NUMBER(����-> ���ڵ����ͷ� ��ȯ)

2. TO_DATE(����-> ��¥)

3. TO_CHAR(����, ��¥-> ����)


***

null ���� ������ ã��(is null)

-- ������ null���� �л��� ��ȣ�� ����
select stu_no, stu_height from student where stu_height is null;


***


1.NVL
NVL(�μ�1, �μ�2): 
�μ� ���� null ���� üũ�Ͽ� ������ ������ ������ ��ȯ�Ͽ� ��ȯ�ϴ� �Լ�

-> �μ�1�� ���̸� �μ� 2 ��ȯ, ���� �ƴϸ� �μ�1�� ��ȯ

��: 
-- �л� Ű ��ȸ�ϵ� null�̸� 0���� ��ȯ
select nvl(stu_height,0) from student;


2. NVL2(�μ�1, �μ�2, �μ�3)
-> �μ�1�� null�̸� �μ�2, null�� �ƴϸ� �μ�3 ��ȯ


3. NULLIF(�μ�1, �μ�2)
-> �μ�1�� �μ�2�� ���Ͽ� ���� ������ NULL, �ƴϸ� �μ�1 ��ȯ

select nvl(nullif('A','A'), '��' from dual;
-- nullif���� �μ�1,2�� �����Ƿ� null ��ȯ. nvl() �ȿ� null�� �Է�-> nvl���� �μ�1�� null �̹Ƿ� �μ�2(��) ��ȯ



select stu_name from student where substr(stu_name,2);

***

TO_NUMBER ����

select to_char(to_number(1234.5678),'9999.999') from dual;
-- ���: 1234.568
-- 9: ���� �ڸ���



***

�ִ�/�ּҰ�

select max(enr_grade), min(enr_grade) from enrol;

***

Count
count(*) : ��ü ���ڵ� ��(Ʃ�� ��)
count(stu_height) : �л� ���� ��(null ����)

select count(*), count(stu_height) from student;


***


select sum(stu_weight), to char(avg(stu_weight),'9999.99' from student where stu_dept='��ǻ������';


***

<group by>: �׷캰�� ���� �� ����

-- �а����� ü���� ��� ���
select stu_dept, avg(stu_weight) from student group by stu_dept;

-- �г� �� �а����� ������ 50 �̻��� �л� �׷���. 
select stu_dept, count(*) from student where stu_weight>=50 group by stu_dept, stu_grade;

***

<having ��>: �׷��Լ��� ������ ����� �ٽ� ������ �ο��� �� ���

select stu_grade, avg(stu_height) from student where stu_dept='���' -- ���� �л����� �г�� Ű ��� ����
group by stu_grade -- �г⺰�� �׷� ������ ����
having avg(stu_height)>=160 -- Ű�� 160 �̻��� �л� ��



***

<lpad>: �ڸ����� �����ϰ� �� ������ Ư�����ڷ� ���ʺ��� ä���(1����Ʈ��)
-- �ѱ��� 2����Ʈ���� �� 6����Ʈ�̹Ƿ� &�� 4�� ��
select lpad(stu_name,10,'&') from student;

<rpad>: �����ʺ��� ä��
select rpad(stu_name,10,'&') from student;


***

<instr>: Ư�� ���ڿ��� ��ġ�� ã�� ��ȯ

--58. �л����� �̸��� '��'�� ���° �ִ��� �˻��϶�.
select stu_name, instr(stu_name,'��') from student;




****

<join>: ��ȣ ���ü��� ���� �ΰ� �̻��� ���̺��κ��� ���ο� ���̺��� �����ϴµ� ���ȴ�

*��������(EQUI): where������ '=' ������ ���

*natural join(�ڿ�����):
select stu_no, stu_naem, enr_grade from student natural join enrol;

-- �й�, �̸�, �а�, ���� ���(�й��� student�� enrol �ΰ��� �� �ֱ� ������ ��𿡼� ������ ���� ��������� ��-> student.stu_no)

