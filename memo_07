class Book{
	String title;  // �ʵ�
	int price;
	
	void show() { // �޼ҵ�
		System.out.println(title+" "+price);
	}
	
	Book(){  // ��ȣ �� ����ִ�(�Ķ���Ͱ� ���� ���) �⺻ ������ 1
		this("",0); // �ٸ� ������ ȣ��
		System.out.println("������ ȣ��!");
	}
	Book(String title){  // �Ķ���Ͱ� �ִ� ������ 2
		this(title,0); // �ٸ� ������ ȣ��
	}
	Book(String title, int price){ // �Ķ���� �ΰ� �ִ� ������ 3
		this.title=title;
		this.price=price;
		
	}
}

public class TT {
	public static void main(String[] args) {

		Book b=new Book("������",30000); // 3�� �����ڷ� ����
		Book b1=new Book("�Ű�"); // 2�� �����ڷ� ����
		Book b2=new Book(); // 1�� �����ڷ� ����
		// �������� ������ ������ ������ �޶� ���� �̸��� �����ڸ� ������ ���� ����
		// 1�� �����ڴ� �� ���� �ٸ� �����ڸ� ȣ���� �� ���. 
		
		b.show();
		b1.show();
		b2.show();
		// ��½� �⺻ �����ں��� �����Ͽ� ������1�� �ִ� syso ���� ��� �� �Լ����
// �⺻�����ڿ� �Ķ���Ͱ� ������ 
//�ٸ� �����ڿ� �Ķ���Ͱ� �־� 
//�� �������� �Ķ���͸� ���� �ͼ� ��� ����. 
//�� �Էµ� ���� �����Ƿ� null, 0���� �ʱ�ȭ ��
		
	}

}


