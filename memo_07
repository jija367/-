class AA{
	public String toString() {
		return "AA";
	}
}
class BB{
	public String toString() {
		return "BB";
	}
}
class In{ // Ŭ������ ���׸��� �ƴ϶� �Լ��� ���׸�
	<T>void show(T t){
		System.out.println(t);
	}
	public String toString() {
		return "CC";
	}
	void show(){
		System.out.println("generic");
	}
}
public class GGG {
	public static void main(String[] args) {
		AA a = new AA();
		BB b = new BB();
		
		In i=new In();  // �Լ� show �տ� <>�� �پ��־� �Լ��� ���׸���
		i.<AA>show(a);  // show �� InŬ������ �ִ� �Լ���
		i.<BB>show(b);
		i.show(i);
		i.show(); // �� show�ʹ� �ٸ� �Լ�(�����ε�)
		
	}

}

