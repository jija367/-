LinkedList ����


class Box<T>{
	Box<T> next;  // ���׸� Ŭ���� ��ü
	T t;
	void in(T t) {
		this.t=t;
	}
	T out() {
		return t;
	}
}
public class GGG {
	public static void main(String[] args) {
		Box<Integer> b=new Box<Integer>();
		b.in(5); // �Լ��� �����Ͽ� integer �� �Է�
		b.next=new Box<Integer>(); // Box<Integer> �� ���� ��ü ����
		b.next.in(40); // ��ü�� �ѹ� �� ����
		
		b.next.next=new Box<Integer>();
		b.next.next.in(50);
		
		Box<Integer> tmp;
		tmp=b.next;
		
		System.out.println(tmp.out());
		// 40, ���		
	}
}

