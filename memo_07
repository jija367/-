import java.util.Scanner;

interface Day {
	int THU = 1, FRI = 2, SAT = 3; // �������̽� ���� �ʵ尪�� final�� ����
}

public class TT {

	public static void main(String[] args) {

		System.out.println("���� �������� ����?");
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		switch (a) {
		case Day.THU:
// ���� ���ڸ� �Է����� �ʾƵ� �������̽��� �̹� ���� ���� �Ǿ��־� 1�� �ν�
			System.out.println("��");
			break;
		case Day.FRI:
			System.out.println("��");
			break;
		case Day.SAT:
			System.out.println("��");
			break;
		default:
		}

	}

}

