import java.util.Scanner;

class Rectangle{
	private int width, height;
	int wh(int width, int height) {
		this.width=width; this.height=height;
		return width*height;
	}
	
}
public class GGG {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		while(true) {
		try {
			System.out.println("�ʺ�, ���� �Է�");
			System.out.println(new Rectangle().wh(s.nextInt(),s.nextInt()));
			break;
		}catch(Exception e) {
			System.out.println("�Ǽ��� �ȵ�");
			s.next();
			continue;
		}
		
		}
		
	}
}


