<�ܷ�Ű ����>

create table t_emp2 (t_empno number(4) not null, t_ename varchar2(40), t_job varchar2(9), t_mgr number(4), 
t_hiredate date, t_sal number(7,2), t_comm number(7,2),
t_deptno number(2) constraint  t_emp2_fk references dept(deptno),
constraint t_emp2_pk primary key(t_empno));


* �ܷ�Ű ���� ���� ��Ȱ��ȭ(disable)
alter table t_emp2 disable constraint t_emp2_fk;

* �ܷ�Ű Ȱ��ȭ(enable)
alter table t_emp2 enable constraint t_emp2_fk;

* �ܷ�Ű ����(drop)
alter table t_emp2 drop constraint t_emp2_fk;


