import java.util.HashMap;
import java.util.Scanner;

public class GGG {
	public static void main(String[] args) {
	
		HashMap<String,Integer> h=new HashMap<>();
		h.put("�Ƹ޸�ī��",4500);
		h.put("īǪġ��",5000);
		h.put("ī���",5500);
		
		Scanner s=new Scanner(System.in);
		while(true) {
			System.out.println("�̸�");
			String a=s.next();
			if(h.containsKey(a)){
				System.out.println(h.get(a));
			}
			else if(a.equals("�׸�")) {
				break;
			}
			else {
				System.out.println("����");
			}
		}
		
	
	}
}


