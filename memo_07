public class GGG {
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		Vector<Integer> v=new Vector<>();
		for(int i=0;i<5;i++) {
			v.add(s.nextInt());
		}
		int sum=0;
		Iterator<Integer> it=v.iterator();
		while(it.hasNext()) {
			sum+=it.next();
		}
		System.out.println(sum);
	}
}


