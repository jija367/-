import java.util.StringTokenizer;

public class TT {
	
	public static void main(String[] args) {
		
	String p="1+2+3";
	StringTokenizer s=new StringTokenizer(p,"+");
	while(s.hasMoreTokens()) { // s�� ����� ���� ���� ������
		System.out.println(s.nextToken());
	}
	
	}
}

